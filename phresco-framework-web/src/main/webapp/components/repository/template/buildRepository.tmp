<div class="optiontitle">
	<div class="unit_text">Build Repo</div>
</div>
<section class="features_content_main">
	<div id="messagedisp" class="alert alert-block hideContent" style="text-align: center; margin: auto 0;">No builds available</div>
	<div class="tree_view tree"></div>
	<div class="file_view hideContent">
		<div class="bs-docs-example build_repo_tab" id="infoTabDiv">
			<ul class="nav nav-tabs" id="myTab">
				<li class="active"><a data-toggle="tab" href="#maven_info">Maven Information</a></li>
				<li><a data-toggle="tab" href="#artifact_info">Artifact Information</a></li>
			</ul>		
		</div>
		<div class="tab-content" id="myTabContent">
			<div id="maven_info" class="tab-pane fade active in">
				<table class="build_repo_table">
					<tr>
						<td class="build_repo_font">Group : </td>
						<td id="groupId"></td>
					</tr>
					<tr>
						<td class="build_repo_font">Artifact : </label></td>
						<td id="artifactId"></td>
					</tr>
					<tr>
						<td class="build_repo_font">Version : </label></td>
						<td id="artifactVersion"></td>
					</tr>
					<tr>
						<td class="build_repo_font">Extension : </label></td>
						<td id="artifactType">jar</td>
					</tr>
					<tr>
						<td class="build_repo_font">XML : </label></td>
						<td>
							<textarea class="popuptextarea" id="artifactXml"></textarea>
						</td>
					</tr>
				</table>
			</div>
			<div id="artifact_info" class="tab-pane fade">
				<table class="build_repo_table">
					<tr>
						<td class="build_repo_font">Repository Path : </label></td>
						<td id="repoPath"></td>
					</tr>
					<tr>
						<td class="build_repo_font">Uploaded By : </label></td>
						<td id="uploader"></td>
					</tr>
					<tr>
						<td class="build_repo_font">Size : </label></td>
						<td id="size"></td>
					</tr>
					<tr>
						<td class="build_repo_font">Uploaded Date : </label></td>
						<td id="uploadedDate"></td>
					</tr>
					<tr>
						<td class="build_repo_font">Last Modified : </label></td>
						<td id="modifiedDate"></td>
					</tr>
				</table>
				<div class="flt_right">
					<input type="button" name="downloadArtifact" value="Download" class="btn btn_style downoad_icon" style="margin-right:10px;">
				</div>	
			</div>	
		</div>		
	</div>
</section>
<section class="content_end hideContent">
	<div class="msgdisplay error"></div>
</section>