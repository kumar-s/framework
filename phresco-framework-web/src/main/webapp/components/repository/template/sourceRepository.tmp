			</tr>
			<tr>
				<td>Build Type</td>
			</tr>
			<tr>
				<td colspan="3">
					<select class="type_build">
						<option>Sprint</option>
					</select>
				</td>
			</tr>
			<tr>
				<td>Week Start</td>
			</tr>
			<tr>
				<td colspan="3">
					<select class="type_build">
						<option>30</option>
					</select>
				</td>
			</tr>
		</table>
		<div align="right"><input type="button" class="btn btn_style dyn_popup_close_ok" value="OK"></div>     
 	</div>
</section>
