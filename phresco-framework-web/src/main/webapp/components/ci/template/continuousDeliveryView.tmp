<section class="optiontitle qual_unit">
	<div class="unit_text" data-i18n="ci.continuousintegration"> </div>
	<div class="flt_right">
		<div class="unit_path">
			<a class = "aliveOpts"> <input id="createContinuousDelivery" type="submit" value="Create a New Continuous Delivery" data-i18n="[value]ci.create"class="btn btn_style"></a>
			<a class = "deadOpts"> <input id="createContinuousDelivery" type="submit" value="Create a New Continuous Delivery" data-i18n="[value]ci.create"class="btn btn_style" disabled></a>
		</div>    
	</div>   
</section>
    
<section class="content_main code_content">
 {{#if projectDelivery}}
     	{{#if projectDelivery.continuousDeliveries}}
     		{{#each projectDelivery.continuousDeliveries}}
    	<div class="widget_testing" name="{{name}}">
        		<h4>{{name}} 
        		<span style="float: none; margin-left: 158px;">[ {{envName}} ]</span> 
                	
                	<span class="aliveOpts">
                    	<a temp="clone" name="{{name}}" class="clone_popup" title="" data-placement="bottom" data-toggle="tooltip" href="#" data-original-title="clone">
                    		<img src="themes/default/images/helios/stack.png" width="29" height="23" border="0" alt="">
                    	</a>
                    	<a class="tooltiptop" title="" data-placement="bottom" data-toggle="tooltip" continuousName="{{name}}" name="editContinuousDelivery" data-original-title="Edit">
                    		<img src="themes/default/images/helios/edit_row_icon.png" width="29" height="23" border="0" alt="">
                    	</a> 
                    	<a temp="deleteCI" name="{{name}}" class="deleteCI" title="" data-placement="bottom" data-toggle="tooltip" href="#" data-original-title="Delete">
                    		<img src="themes/default/images/helios/delete_row_icon.png" width="29" height="23" border="0" alt="0">
                    	</a>
                	</span>
                	
                	<span class="deadOpts">
                    	<a name="{{name}}" title="" data-placement="bottom" data-toggle="tooltip" href="javascript:;" data-original-title="clone">
                    		<img src="themes/default/images/helios/stack_dis.png" width="29" height="23" border="0" alt="">
                    	</a>
                    	<a title="" data-placement="bottom" data-toggle="tooltip" continuousName="{{name}}" data-original-title="Edit">
                    		<img src="themes/default/images/helios/edit_row_icon_dis.png" width="29" height="23" border="0" alt="">
                    	</a> 
                    	<a name="{{name}}" title="" data-placement="bottom" data-toggle="tooltip" href="javascript:;" data-original-title="Delete">
                    		<img src="themes/default/images/helios/delete_row_icon_off.png" width="29" height="23" border="0" alt="0">
                    	</a>
                	</span>
                	
                </h4>
    			<div class="widget_testing_env">
    			 {{#each jobs}}
                    	<div id="{{jobName}}"class="pipeline_box">
                            	<img class="img_process" src="themes/default/images/helios/queue_icon.png">{{operation}}
                          	<div >
	                          	<span class="aliveOpts">
	                                <a href="javascript:;" class="ci_info execute_button"><img class="wait_button" src="themes/default/images/helios/wait.png"></a>
	                                 <a href="javascript:;" class="wait_button" temp="generate_build" jobName="{{jobName}}" projectId="{{projectDelivery.id}}"><img class="execute_button" src="themes/default/images/helios/execute_icon.png"></a>
	                                <a temp="list_builds" jobName="{{jobName}}" projectId="{{projectDelivery.id}}" href="javascript:;" class="datetime_status"><img class="time_button" src="themes/default/images/helios/time.png"></a>
	                             </span>
	                             <span class="deadOpts">
	                                <a href="javascript:;" class="wait_button"><img src="themes/default/images/helios/wait.png"></a>
	                                 <a href="javascript:;" class="wait_button" jobName="{{jobName}}" projectId="{{projectDelivery.id}}"><img src="themes/default/images/helios/execute_icon.png"></a>
	                                <a jobName="{{jobName}}" projectId="{{projectDelivery.id}}" href="javascript:;"><img src="themes/default/images/helios/time.png"></a>
	                             </span>
                        	</div>
                           
                            
                       	</div>
           		 {{/each}}
                        <div class="clear"></div>
                 </div>
          </div>
  		
          
        <div id="datetime_status" class="dyn_popup popup_bg">
        	<table class="table table-striped table_border table-bordered" cellpadding="0" cellspacing="0" border="0">
              <thead>
                <tr>
                  <th data-i18n="ci.datetime"></th>
                  <th data-i18n="ci.status"></th>
                  <th data-i18n="ci.delete"></th>
                </tr>
              </thead>
              <tbody name="buildList">
                 
                  
              </tbody>
        	</table>
        </div>
        <div id="ci_info" class="dyn_popup popup_bg">
            <div class="ci_inner_info">
                <h1>Info</h1>
                <div>
                	<b>Executed:</b> Apr 18, 2012, 12:45PM<br>
                    <b>Duration:</b> 18 Mins 15 Secs<br>
                    <b>Status:</b> <span class="completedinfo">Completed</span>
                </div> 
             </div> 
        </div>
        {{#each jobs}}
       
        {{/each}}
        	           {{/each}}
          {{/if}} 
        {{else}}
            <div class="alert alert-block" style="text-align: center; margin: auto 0;" data-i18n="ci.cdjobsnotavailable"></div>
        {{/if}}          
         <div id="clone_popup" class="dyn_popup">
         <form>
            <ul>
                <li data-i18n="configuration.label.name"><br/></li>
                <li>
                    <input type="text" name="cloneName">
                </li>
                <li data-i18n="ci.environment"><br/></li>
                <li>
                    <select name="envName">
                        
                    </select>
                </li>
                <li>
                    <div class="flt_left">
                    	<input type="hidden" name="continuousName">
                        <input type="button" id="clone_ci" value="Clone" data-i18n="[value]configuration.label.clone" class="btn btn_style">
                        <input type="button" value="Close" data-i18n="[value]common.btn.close" class="btn btn_style dyn_popup_close">
                    </div>
                </li>
            </ul>
            </form>
        </div>
        <div id="deleteCI" class="dyn_popup dyn_popup_delete"> Are you sure to delete this ?
            <div>
            	<input type="hidden" name="cdName">
                <input type="button" value="Yes" data-i18n="[value]common.btn.yes" class="btn btn_style"  name="confirmDeleteCI">
                <input type="button" value="No" data-i18n="[value]common.btn.no" class="btn btn_style dyn_popup_close">
            </div>
        </div>
    </section>