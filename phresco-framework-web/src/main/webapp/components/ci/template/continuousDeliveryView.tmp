<section class="optiontitle qual_unit">
        <div class="unit_text">
            Continuous Delivery
        </div>
        <div class="flt_right">
            <div class="unit_path">
                <a> <input id="createContinuousDelivery" type="submit" value="Create a New Continuous Delivery" class="btn btn_style"></a>
            </div>    
         </div>   
        
    </section>
    
<section class="content_main code_content">
 {{#if projectDelivery}}
     	{{#if projectDelivery.continuousDeliveries}}
     		{{#each projectDelivery.continuousDeliveries}}
    	<div class="widget_testing">
        		<h4>{{name}}
                	<span>
                    	<a temp="clone" name="{{name}}" class="clone_popup" title="" data-placement="bottom" data-toggle="tooltip" href="#" data-original-title="clone">
                    		<img src="themes/default/images/helios/stack.png" width="29" height="23" border="0" alt="">
                    	</a>
                    	<a class="tooltiptop" title="" data-placement="bottom" data-toggle="tooltip" name="editContinuousDelivery" data-original-title="Edit">
                    		<img src="themes/default/images/helios/edit_row_icon.png" width="29" height="23" border="0" alt="">
                    	</a> 
                    	<a temp="deleteCI" name="{{name}}" class="deleteCI" title="" data-placement="bottom" data-toggle="tooltip" href="#" data-original-title="Delete">
                    		<img src="themes/default/images/helios/delete_row_icon.png" width="29" height="23" border="0" alt="0">
                    	</a>
                	</span>
                	
                </h4>
    			<div class="widget_testing_env">
    			 {{#each jobs}}
                    	<div class="pipeline_box">
                            	<img class="img_process" src="themes/default/images/helios/queue_icon.png">{{operation}}
                          	<div>
                                <a href="javascript:;" class="ci_info"><img src="themes/default/images/helios/wait.png"></a>
                                 <a href="javascript:;"  temp="generate_build" jobName="{{jobName}}" projectId="{{projectDelivery.id}}"><img src="themes/default/images/helios/wait.png"></a>
                                <a temp="list_builds" jobName="{{jobName}}" projectId="{{projectDelivery.id}}" href="javascript:;" class="datetime_status"><img src="themes/default/images/helios/time.png"></a>
                            </div>
                       	</div>
           		 {{/each}}
                        <div class="clear"></div>
                 </div>
          </div>
  		
          
        <div id="datetime_status" class="dyn_popup popup_bg">
        	<table class="table table-striped table_border table-bordered" cellpadding="0" cellspacing="0" border="0">
              <thead>
                <tr>
                  <th>Date & Time</th>
                  <th>Status</th>
                  <th>Delete</th>
                </tr>
              </thead>
              <tbody name="buildList">
                 
                  
              </tbody>
        	</table>
        </div>
        <div id="ci_info" class="dyn_popup popup_bg">
            <div class="ci_inner_info">
                <h1>Info</h1>
                <div>
                	<b>Executed:</b> Apr 18, 2012, 12:45PM<br>
                    <b>Duration:</b> 18 Mins 15 Secs<br>
                    <b>Status:</b> <span class="completedinfo">Completed</span>
                </div> 
             </div> 
        </div>
        {{#each jobs}}
       
        {{/each}}
        	           {{/each}}
          {{/if}} 
        {{else}}
            <div class="alert alert-block" style="text-align: center; margin: auto 0;">Continuous Delivery Jobs not available</div>
        {{/if}}          
         <div id="clone_popup" class="dyn_popup">
         <form>
            <ul>
                <li>Name<br/></li>
                <li>
                    <input type="text" name="cloneName">
                </li>
                <li>Environment<br/></li>
                <li>
                    <select>
                        <option>Production</option>
                    </select>
                </li>
                <li>
                    <div class="flt_left">
                    	<input type="hidden" name="continuousName" class="btn btn_style dyn_popup_close">
                        <input type="button" id="clone_ci" value="Clone" class="btn btn_style">
                        <input type="button" value="Close" class="btn btn_style dyn_popup_close">
                    </div>
                </li>
            </ul>
            </form>
        </div>
        <div id="deleteCI" class="dyn_popup dyn_popup_delete"> Are you sure to delete this ?
            <div>
            	<input type="hidden" name="cdName">
                <input type="button" value="Yes" class="btn btn_style"  name="confirmDeleteCI">
                <input type="button" value="No" class="btn btn_style dyn_popup_close">
            </div>
        </div>
    </section>