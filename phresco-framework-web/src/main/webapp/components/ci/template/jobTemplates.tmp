    <section class="optiontitle qual_unit">
      <div class="unit_text">
          Job Templates
        </div>
        <div class="flt_right">
        <input type="submit" value="Create a Job Template" name="jobTemplatePopup" class="btn btn_style create_job_btn">
        <div id="jobTemplatePopup" class="dyn_popup">
          <form id="jobTemplate">
                        <table class="table" cellpadding="0" cellspacing="0" border="0">
                            <tbody>
                                <tr>
                                  <td>Name</td>
                                    <td>Job Type</td>
                                </tr>
                                <tr>
                                  <td><input type="text" placeholder="Job Name" name="name" value=""></td>
                                    <td><select name="type">
                                              <option value="Code Validation">Code Validation</option>
                                                <option value="Build">Build</option>
                                                <option value="Deploy">Deploy</option>
                                                <option value="Functional Test">Functional Test</option>
                                                <option value="Unit Test">Unit Test</option>
                                        </select>
                                    </td>
                                </tr>
                                <tr>
                                  <td>Application<sup>*</sup></td>
                                </tr>
                                 <tr>
                                  <td colspan="3">
                                      <div class="popuptextarea" id="appIdsList">
                                            <input type="checkbox" name="appIds" value="jqueryWidgetApp">JQW - Jquery Widget<br>
                                            <input type="checkbox" name="appIds" value="NativeEshopApp">Native_eshop-androidnative<br>
                                            <input type="checkbox" name="appIds" value="NodeJsApp">ds-nodejswebservice
                                        </div>
                                    </td>
                                 </tr>
                            </tbody>
                         </table>
                         <table id="features" class="table table_template" cellpadding="0" cellspacing="0" border="0">
                            <tbody>
                                <tr>
                                    <td><input name="enableRepo" type="checkbox">Repo</td>
                                    <td><select name="repoTypes">
                                              <option value="svn">SVN</option>
                                              <option value="git">Git</option>
                                              <option value="bitkeeper">Bitkeeper</option>
                                        </select>
                                    </td>
                                </tr>
                                <tr>
                                    <td><input type="checkbox" name="enableSheduler">Scheduler</td>
                                </tr>
                                <tr>
                                    <td><input type="checkbox" name="enableEmailSettings">Email Settings</td>
                                </tr>
                                <tr>
                                    <td><input type="checkbox" name="enableUploadSettings">Uplaod Settings</td>
                                </tr>
                            </tbody>
                          </table>
                         
                          <div class="flt_right">
                            <input type="button" value="Create" name="save" class="btn btn_style">
                            <input type="button" value="Close" class="btn btn_style dyn_popup_close">
                         </div>
            </form>
        </div>
  </div>
    </section>
    <section class="features_content_main create_proj">
        {{#if jobTemplates}}
          <table id="jobTemplateList" class="table table-striped table_border table-bordered jobs_table" cellpadding="0" cellspacing="0" border="0">
              <thead>
                <tr>
                  <th>Name</th>
                  <th>Type</th>
                  <th>Application</th>
                  <th>Configure</th>
                  <th>Delete</th>
                </tr>
              </thead>
              <tbody>
                  {{#jobTemplates}}
                    <tr>
                      <td>{{name}}</td>  
                      <td>{{type}}</td>
                      <td>{{appIds}}</td>
                      <td>
                          <a value="{{name}}" data-original-title="Configure" id="edit_{{name}}" name="editpopup" href="#" data-toggle="tooltip" data-placement="top" title="" class="tooltiptop"><img width="29" height="23" border="0" alt="" src="themes/default/images/helios/edit_row_icon.png"></a>
                      </td>
                      <td>
                          <a value="{{name}}" class="tooltiptop" id="delete_{{name}}" name="deleteconfirm" title="" data-placement="top" data-toggle="tooltip" href="#" data-original-title="Delete"><img src="themes/default/images/helios/delete_row.png" width="14" height="18" border="0" alt=""></a>
                          
                          <div id="yesnopopup_{{name}}" name="{{name}}" class="dyn_popup"> Are you sure to delete this ?
                          <div>
                            <input type="button" value="Yes" class="btn btn_style" name="delete">
                            <input type="button" value="No" class="btn btn_style dyn_popup_close">
                          </div>
                      </td>
                    </tr>
                  {{/jobTemplates}}
              </tbody>
        </table>
        {{else}}
            <div>No Job templates available</div>
        {{/if}}
        </div>
      <!--content_main div ends here -->

    </section>