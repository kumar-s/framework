    <section class="optiontitle qual_unit">
      <div class="unit_text" data-i18n="ci.jobtemplates"/>                  
        <div class="flt_right">
        <input type="submit" data-i18n="[value]ci.createjobtemplate;" name="jobTemplatePopup" class="btn btn_style create_job_btn">
        <div id="jobTemplatePopup" class="dyn_popup job_template">
          <form id="jobTemplate">
                        <table class="table" cellpadding="0" cellspacing="0" border="0">
                            <tbody>
                                <tr>
                                  <td data-i18n="ci.name"/>
                                  <td data-i18n="ci.type"/>
                                </tr>
                                <tr>
                                  <td>
                                    <input type="text" placeholder="Job Name" name="name" value="">
                                    <input type="hidden" name="oldname" value="">
                                  </td>
                                  <td>
                                      <select name="type" class="selectpicker">
                                            <option value="codeValidation">Code Validation</option>
                                            <option value="build">Build</option>
                                            <option value="deploy">Deploy</option>
                                            <option value="unittest">Unit Test</option>
                                            <option value="componentTest">Component Test</option>
                                            <option value="functionalTest">Functional Test</option>
                                            <option value="performanceTest">Performance Test</option>
                                            <option value="loadTest">Load Test</option>
                                            <option value="pdfReport">PDF Report</option>
                                      </select>
                                  </td>
                                </tr>
                                <tr>
									<td data-i18n="ci.application" colspan="2"><sup>*</sup></td>
                                </tr>
								<tr>
									<td colspan="2"><select id="appIds" name="appIds" class="selectpicker" multiple data-selected-text-format="count>1"></select></td>
								</tr>
							</tbody>
                         </table>
                         <table id="features" class="table table_template" cellpadding="0" cellspacing="0" border="0">
                            <tbody>
                                <tr>
                                    <td><input name="enableRepo" type="checkbox">Repo</td>
                                    <td><select name="repoTypes" class="selectpicker">
                                              <option value="svn">SVN</option>
                                              <option value="git">Git</option>
                                        </select>
                                    </td>
                                </tr>
                                <tr>
                                    <td><input type="checkbox" name="enableSheduler">Scheduler</td>
                                </tr>
                                <tr>
                                    <td><input type="checkbox" name="enableEmailSettings">Email Settings</td>
                                </tr>
                                <tr>
                                    <td><input type="checkbox" name="enableUploadSettings">Upload Settings</td>
									<td>
										<select name="uploadTypes" class="selectpicker" multiple data-selected-text-format="count>1">
										  <option value="Collabnet">Collabnet</option>
										  <option value="Confluence">Confluence</option>
                      <option value="Cobertura">Cobertura</option>
										</select>
									</td>
                                </tr>
                            </tbody>
                          </table>
						  <span id='errMsg' class="flt_left"/>						
                          <div class="flt_right">							
							<input type="button" data-i18n="[value]ci.create;" name="save" class="btn btn_style">
                            <input type="button" data-i18n="[value]ci.close;" class="btn btn_style dyn_popup_close">
                         </div>
            </form>
        </div>
  </div>
    </section>
    <section class="features_content_main create_proj">
        {{#if jobTemplates}}
          <table id="jobTemplateList" class="table table-striped table_border table-bordered jobs_table" cellpadding="0" cellspacing="0" border="0">
              <thead>
                <tr>
                  <th data-i18n="ci.name"/>
                  <th data-i18n="ci.type"/>
                  <th data-i18n="ci.application"/>
                  <th data-i18n="ci.edit"/>
                  <th data-i18n="ci.delete"/>
                </tr>
              </thead>
              <tbody>
                  {{#jobTemplates}}
                    <tr>
                      <td>{{name}}</td>  
                      <td>{{type}}</td>
                      <td>{{appIds}}</td>
                      <td>
                          <a value="{{name}}" data-original-title="Edit" id="edit_{{name}}" name="editpopup" href="#" data-toggle="tooltip" data-placement="top" title="" class="tooltiptop"><img width="29" height="23" border="0" alt="" src="themes/default/images/helios/edit_row_icon.png"></a>
                      </td>
                      <td>
                          <a value="{{name}}" class="tooltiptop" id="delete_{{name}}" name="deleteconfirm" title="" data-placement="top" data-toggle="tooltip" href="#" data-original-title="Delete"><img src="themes/default/images/helios/delete_row.png" width="14" height="18" border="0" alt=""></a>
                          
                          <div id="yesnopopup_{{name}}" name="{{name}}" class="dyn_popup"> Are you sure to delete this ?
                          <div>
                            <input type="button" value="Yes" class="btn btn_style" name="delete">
                            <input type="button" value="No" class="btn btn_style dyn_popup_close">
                          </div>
                      </td>
                    </tr>
                  {{/jobTemplates}}
              </tbody>
        </table>
        {{else}}
            <div class="alert alert-block" style="text-align: center; margin: auto 0;">Job Templates not available</div>
        {{/if}}
        </div>
      <!--content_main div ends here -->

    </section>