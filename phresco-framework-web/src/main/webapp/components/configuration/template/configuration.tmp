<div id="configurationPage">
	<div class="second_title">
		<div class="code_anal">
			<input type="submit" name="env_pop" data-i18n="[value]configuration.label.addenvironment"  class="btn btn_style">
			 <div id="env_pop" class="dyn_popup popup_bg" style="display:none;">
                <table width="100%" border="0" cellspacing="0" cellpadding="5">
                <tr>
                	<td>Name <sup>*</sup></td>
                    <td>Description <sup>*</sup></td>
                    <td>&nbsp;</td>
                </tr>
                <tr>
                	<td><input type="text" class="pop_txt" placeholder="Environment Name"></td>
                    <td><input type="text" class="pop_txt" placeholder="Environment Description"></td>
                    <td><input type="button" class="btn btn_style" value="Add"></td>
                </tr>
                <tr>
                	<td colspan="3">
                    	<div class="env_border">	                        	
                            <table class="table table-striped table_border table-bordered add_env" cellpadding="0" cellspacing="0" border="0">
                            <thead>
                                <tr>
                                	<th>Default</th>
                                    <th>Environment</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td colspan="2">
                                        <ul class="connected list" id="content_1">
                                            <li>
                                                <div><input type="radio" name="optionsRadiosfd" checked></div>
                                                <div>Production (Default)</div>
                                            </li>
                                            <li>
                                                <div><input type="radio" name="optionsRadiosfd"></div>
                                                <div>Testing</div>
                                            </li>
                                            <li>
                                                <div><input type="radio" name="optionsRadiosfd"></div>
                                                <div>Staging</div>
                                            </li>
                                            <li>
                                                <div><input type="radio" name="optionsRadiosfd"></div>
                                                <div>Test</div>
                                            </li>
                                        </ul>
                                    </td>
                                </tr>
                            </tbody>
                          </table>
                            
                            <!--env_border div ends here -->	
                        </div>
                        <div class="flt_right">
                            <input type="button" value="Ok" class="btn btn_style">
                            <input type="button" value="Close" class="btn btn_style dyn_popup_close">
                        </div>
                    </td>
                </tr>
                </table>
                <!--env_pop div ends here -->	
            </div>
			<!--code_anal div ends here -->
		</div>
		<!--second_title div ends here -->
	</div>
		
	<section class="features_content_main">
		<table class="table table-striped table_border table-bordered" cellpadding="0" cellspacing="0" border="0">
			  <thead>
				<tr>
				  <th data-i18n="configuration.label.name"></th>
				  <th data-i18n="configuration.label.description"></th>
				  <th data-i18n="configuration.label.configure"></th>
				  <th data-i18n="configuration.label.clone"></th>
				  <th>&nbsp;</th>
				</tr>
			  </thead>
			  <tbody>
				{{#configurationList}}
			  <tr>
				<td>{{name}}</td>  
				<td>{{desc}}</td>
				<td><a href="#" name="editConfiguration" key={{name}}><img src="../themes/default/images/helios/configure_icon.png" width="15" height="16" border="0" alt=""></a></td>
				<td>
					<a class="tooltiptop" href="#" data-toggle="tooltip" data-original-title="Clone" name="clone_pop"><img src="../themes/default/images/helios/clone_icon.png" width="20" height="18" border="0" alt=""></a>
					<div id="clone_pop" class="dyn_popup popup_bg" style="display:none;">
                <table width="100%" border="0" cellspacing="0" cellpadding="5">
                <tr>
                	<td>Name <sup>*</sup></td>
                    <td>Description <sup>*</sup></td>
                    <td>&nbsp;</td>
                </tr>
                <tr>
                	<td><input type="text" class="pop_txt" placeholder="Environment Name"></td>
                    <td><input type="text" class="pop_txt" placeholder="Environment Description"></td>
                    <td><input type="button" class="btn btn_style" value="Add"></td>
                </tr>
                <tr>
                	<td colspan="3">
                    	<div class="env_border">	                        	
                            <table class="table table-striped table_border table-bordered add_env" cellpadding="0" cellspacing="0" border="0">
                            <thead>
                                <tr>
                                	<th>Default</th>
                                    <th>Environment</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td colspan="2">
                                        <ul class="connected list" id="content_1">
                                            <li>
                                                <div><input type="radio" name="optionsRadiosfd" checked></div>
                                                <div>Production (Default)</div>
                                            </li>
                                            <li>
                                                <div><input type="radio" name="optionsRadiosfd"></div>
                                                <div>Testing</div>
                                            </li>
                                            <li>
                                                <div><input type="radio" name="optionsRadiosfd"></div>
                                                <div>Staging</div>
                                            </li>
                                            <li>
                                                <div><input type="radio" name="optionsRadiosfd"></div>
                                                <div>Test</div>
                                            </li>
                                        </ul>
                                    </td>
                                </tr>
                            </tbody>
                          </table>
                            
                            <!--env_border div ends here -->	
                        </div>
                        <div class="flt_right">
                            <input type="button" value="Ok" class="btn btn_style">
                            <input type="button" value="Close" class="btn btn_style dyn_popup_close">
                        </div>
                    </td>
                </tr>
                </table>
                <!--env_pop div ends here -->	
            </div>
				</td>
				<td><a class="tooltiptop" title="" data-placement="top" data-toggle="tooltip" href="#" data-original-title="Delete Row"><img src="../themes/default/images/helios/delete_row_icon.png" width="29" height="23" border="0" alt=""></a></td>
			  </tr>
			{{/configurationList}}
			  </tbody>
		</table>
		<!--features_content_main div ends here -->
	</section>

</div>	