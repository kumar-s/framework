<section class="add_config">
		<div class="second_title">
				<!--<input type="submit" value="Add Environment" class="btn btn_style"> -->
				
				<div class="navbar navbar-static flt_right">
					<ul class="nav">
						<li id="fat-menu" class="dropdown">
							<a href="#" id="drop3" role="button" class="dropdown-toggle drop-qual" data-toggle="dropdown">
								{{#if userPermissions.manageConfiguration}}
							  		<input type="submit" value="Add Configuration" class="btn btn_style">
						  		{{else}}
					  				<input type="submit" value="Add Configuration" class="btn btn_style" disabled>
					  			{{/if}}
					  		</a>
						  <div class="dropdown-menu cust_sel fix_height" role="menu" aria-labelledby="drop3">
							<ul name="configurations">
								{{#configType}}
								<li name="{{this}}"><a href="#">{{this}}</a></li>
								{{/configType}}
								<li name="Other"><a href="#">Other</a></li>
							</ul>
						  </div>
						</li>
					</ul>
				</div>
				
			<!--second_title div ends here -->
		</div>
		
		<section class="features_content_main tab_line">
			<table class="table table_border table-bordered" cellpadding="0" cellspacing="0" border="0">  
				<tbody name="ConfigurationLists"> 
					<tr>
						{{#configurations}}
							<td>Name</td>
							<td><input type="text" name="EnvName" value="{{name}}"/></td>
							<td>Description</td>
							<td colspan="3"><input type="text" value="{{desc}}"/></td>
						{{/configurations}}
					</tr>
				</tbody>
			</table>
			
			
			
			<!--features_content_main div ends here -->
		</section>
		<section class="content_end">
			<div class="flt_right">
				{{#if userPermissions.manageConfiguration}}
					<input type="submit" name="UpdateConfig" value="Update" class="btn btn_style update_btn">
				{{else}}
					<input type="submit" name="UpdateConfig" value="Update" class="btn btn_style update_btn" disabled>
				{{/if}}
				<input id="cancelEditConfig"" type="submit" value="Cancel" class="btn btn_style cancel_icon_btn">
			</div>
			<!--content_title ends here -->
		</section>
	</section> 