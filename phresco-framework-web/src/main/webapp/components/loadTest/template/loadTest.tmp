<section class="optiontitle qual_unit loadTemp">
	<div class="unit_text" data-i18n="ci.loadtest">
    </div>
    <input type="hidden" class="testingType" value="load-test"/>
        <div class="flt_right">
        	{{#showIcons resultAvailable testResultFiles}}
	        	<div class="unit_view performanceView">
	                <span id="tabularView">
	                	<a href="#"><img class="tableIcon" src="themes/default/images/helios/quality_table_on.png" width="25" height="25" border="0" alt=""></a>
	                    <a href="#"><b>Table View</b></a>
	                </span>
	                <span id="graphicalView">
	                	<a href="#"><img class="graphIcon" src="themes/default/images/helios/quality_graph_off.png" width="26" height="24" border="0" alt=""></a>
	                	<a href="#">Graph View</a>
	                </span>
	            </div>
	        {{else}}  
	        	<div class="unit_view performanceView hideContent">
	                <span id="tabularView">
	                	<a href="#"><img class="tableIcon" src="themes/default/images/helios/quality_table_on.png" width="25" height="25" border="0" alt=""></a>
	                    <a href="#"><b>Table View</b></a>
	                </span>
	                <span id="graphicalView">
	                	<a href="#"><img class="graphIcon" src="themes/default/images/helios/quality_graph_off.png" width="26" height="24" border="0" alt=""></a>
	                	<a href="#">Graph View</a>
	                </span>    
	            </div>  
            {{/showIcons}}

            {{#showIcons resultAvailable testResultFiles}}
	           <div class="icon_images"><a class="tooltiptop loadPDF" from="load" title="" data-placement="bottom" data-toggle="tooltip" href="#" data-original-title="PDF Report" id="pdfIcon"><img src="themes/default/images/helios/pdf_icon_white.png" ></a>
	        		<!--Pdf report popup starts here -->
					<div id="pdf_report" class="dyn_popup hideContent">
						<div id="noPdfReport" class="alert noReport"></div>
	                	<table class="table_border availablePdfRptsTbl hideContent" border="0" cellspacing="0" cellpadding="5" width="100%" id="availablePdfRptsTbl">
	                		<thead>
	                        	<tr>
		                            <th>Existing Reports</th>
		                            <th>Type</th>
		                            <th>Download</th>
		                            <th>Delete</th>
	                        	</tr>    
	                    	</thead>
	                    	<tbody id="availablePdfRptsTbdy">
	                    	    
	                   	 	</tbody>    
	                	</table>
	                	<form id="pdfReportForm">
		                   	<table class="report_type" style="border: none; margin-top:4px;">
		                    	<tbody>
		                        	<tr>
		                            	<td>Report Type</td>
		                            	<td>
		                                	<select name="reportDataType">
		                                    	<option value="detail">Detailed Report</option>
		                                    	<option value="crisp">Overall Report</option>
		                                	</select>
		                            	</td>
		                        	</tr>
									<tr>
		                            	<td>Pdf Report Name</td>
			                            <td>
			                               <input type="text" placeholder="Pdf report name" name="pdfName">
			                            </td>
		                        	</tr>	                        	
		                    	</tbody>
		                   	</table>
		                   	<div class="popuploading" id="pdfReportLoading">&nbsp</div>
			               	<div class="flt_right">
			                 	<input type="button" value="Generate" class="btn btn_style" id="generatePdf">
			                 	<input type="button" value="Close" class="btn btn_style dyn_popup_close">
			                 	<input type="hidden" name="fromPage" value="load"/>
			               	</div>
	           			</form>
					</div>
					<!--Pdf report popup ends here -->
	    		</div>
			{{else}}  
	        	<div class="icon_images hideContent"><a class="tooltiptop" title="" data-placement="bottom" data-toggle="tooltip" href="#" data-original-title="PDF Report" id="pdfIcon"><img src="themes/default/images/helios/pdf_icon_white.png" ></a></div>
            {{/showIcons}}
        	<div class="icon_images performanceScreenShotDiv" style="display: none;"><a class="tooltiptop performanceScreenShot" title="" data-placement="bottom" data-toggle="tooltip" href="#" data-original-title="Screen Shot"><img src="themes/default/images/helios/screenshot_white.png" ></a></div>
            
			<div class="icon_images"><a class="tooltiptop" title="" data-placement="bottom" data-toggle="tooltip" href="#" data-original-title="Open Folder" id="openFolder"><img src="themes/default/images/helios/openfolder_icon.png" ></a></div>
			<div class="icon_images copy_path_icon"><a class="tooltiptop" title="" data-placement="bottom" data-toggle="tooltip" href="#" data-original-title="Copy Path" id="copyPath"><img src="themes/default/images/helios/copypath_icon.png" ></a></div>

			{{#showResultFiles testResultFiles}}
	            <div class="code_report type_report testResultDropdown">
	            	Results
	            </div>
	            <div class="navbar navbar-static code_func logintest testResultDiv">
	                <ul class="nav">
	                    <li id="fat-menu" class="dropdown">
	                      <a href="#" id="testResultFileDrop" role="button" class="dropdown-toggle" data-toggle="dropdown" value={{#testResultFiles testResultFiles true}}{{/testResultFiles}}>{{#testResultFiles testResultFiles true}}{{/testResultFiles}}<b class="caret"></b></a>
	                      <div class="dropdown-menu cust_sel code_test_opt" role="menu" aria-labelledby="drop5">
	                      	<ul id="resultFiles">
	                        	{{#testResultFiles testResultFiles false}}{{/testResultFiles}}
	                        </ul>
	                      </div>
	                    </li>
	                </ul>
	            </div>
	        {{else}}
	        	<div class="code_report type_report hideContent testResultDropdown">
	            	Results
	            </div>
	            <div class="navbar navbar-static code_func logintest hideContent testResultDiv">
	                <ul class="nav">
	                    <li id="fat-menu" class="dropdown">
	                      <a href="#" id="testResultFileDrop" role="button" class="dropdown-toggle" data-toggle="dropdown" value={{#testResultFiles testResultFiles true}}{{/testResultFiles}}>{{#testResultFiles testResultFiles true}}{{/testResultFiles}}<b class="caret"></b></a>
	                      <div class="dropdown-menu cust_sel code_test_opt" role="menu" aria-labelledby="drop5">
	                      	<ul id="resultFiles">
	                        	{{#testResultFiles testResultFiles false}}{{/testResultFiles}}
	                        </ul>
	                      </div>
	                    </li>
	                </ul>
	            </div>    
            {{/showResultFiles}}
            
            {{#showTestAgainst showDevice resultAvailable}}
            <div class="code_report type_report">
            	Type
            </div>
            <div class="navbar navbar-static code_func">
                <ul class="nav testAgainstNav">
                    <li id="fat-menu" class="dropdown">
                      <a href="#" id="testAgainstsDrop" role="button" class="dropdown-toggle" data-toggle="dropdown" value={{#testAgainsts testAgainsts true}}{{/testAgainsts}}>{{#testAgainsts testAgainsts true}}{{/testAgainsts}}<b class="caret"></b></a>
                      <div class="dropdown-menu cust_sel code_test_opt" role="menu" aria-labelledby="drop5">
	                      	<ul>	
	                      		{{#testAgainsts testAgainsts false}}{{/testAgainsts}}
	                        </ul>
                      </div>
                    </li>
                </ul>
            </div>
            {{/showTestAgainst}}
            
            <div class="code_anal">
            	{{#if userPermissions.manageTests}}
               		 <input type="submit" value="Test" data-i18n="[value]test.label.test" name="loadPopup" class="btn btn_style">
				{{else}}               		 
					<input type="submit" value="Test" data-i18n="[value]test.label.test" name="loadPopup" class="btn btn_style" disabled>
				{{/if}}
                <div id="loadPopup" class="dyn_popup performance_pop_cont" style="display:none;">
                    <form name="loadForm" id="loadForm">	
                        <ul class="row dynamicControls">
                        </ul>
                        <div class="templates"></div>
						<div class="hiddenControls"></div>
					</form>
                    <div class="alignright">
                    	<div class="dynamicPopupLoading" style="display:none;">&nbsp</div>
                        <input id="loadRun" type="button" value="Test" class="btn btn_style">
                        <input type="button" value="Close" class="btn btn_style dyn_popup_close">
                    </div>
                </div>
             </div>
        </div>
    </section>
    
    <section class="content_main code_content testSuiteTable performanceTestResults loadTemp">
    	<!-- Table view div starts here -->
			<div class="qual_unit_main unit_info" id="testSuites">
				{{#unless resultAvailable}}
	    		<div style="text-align: center; margin: auto 0;" class="alert alert-block perfError" data-i18n="testresult.label.loadnotexecuted"></div>
			{{else}}
			 	{{#showErrMsg resultAvailable testResultFiles}}
	    			<div style="text-align: center; margin: auto 0;" class="alert alert-block perfError">Test not executed for {{#against testAgainsts}}{{/against}}</div>
		    	{{else}}	
		    		<div style="text-align: center; margin: auto 0; display:none" class="alert alert-block perfError"></div>
		    	{{/showErrMsg}}   		
	    	{{/unless}}
	    		<div class="perfResultInfo"></div>
		    </div>
	    <!-- Table view div ends here -->
    
		<!-- Graphical view div starts here -->
		    <div class="qual_unit_main unit_info hideContent" id="graphView">
		    	<div class="code_report type_report">Based on</div>
		    	<div class="navbar navbar-static code_func logintest graphFor performanceGraphNav">
	                <ul class="nav">
	                    <li class="dropdown" id="fat-menu">
	                      <a value="responseTime" data-toggle="dropdown" class="dropdown-toggle" role="button" id="graphForDrop" href="#">Aggr Response Time<b class="caret"></b></a>
	                      <div aria-labelledby="drop5" role="menu" class="dropdown-menu cust_sel code_test_opt">
	                      	<ul id="graphForList">
	                        	<li class="graphForDrop"><a name="graphForDrop" value="responseTime" href="#">Response Time</a></li>
	                        	<li class="graphForDrop"><a name="graphForDrop" value="throughPut" href="#">Throughput</a></li>
	                        	<li class="graphForDrop"><a name="graphForDrop" value="minResponseTime" href="#">Min Response Time</a></li>
	                        	<li class="graphForDrop"><a name="graphForDrop" value="maxResponseTime" href="#">Max Response Time</a></li>
	                        	<li class="graphForDrop"><a name="graphForDrop" value="all" href="#">All</a></li>
	                        </ul>
	                      </div>
	                    </li>
	                </ul>
	            </div>
		    	<div class="graph_area graph_performance">
		        	<canvas id="allData" width="420" height="300">[No canvas support]</canvas>
		        	<canvas id="myCanvas" width="420" height="300">[No canvas support]</canvas>
		        </div>
		    </div>
	    <!-- Graphical view div ends here -->
    
	     <!-- Progress div starts here-->
		    <div class="unit_close performance_close">
		    	<a href="#" data-flag="true" id="consoleImg">
		    		<img src="themes/default/images/helios/console_img.png" width="33" height="92" border="0" alt="">
		    		<div class="progress_loading" style="display:none;"><img src="themes/default/images/helios/progress_load.gif" border="0"></div>
				</a>
			</div>
		    <div class="unit_progress performance_progress">
		        <table id="unit_progress" class="table table-striped table_border table-bordered" cellpadding="0" cellspacing="0" border="0">
		            <thead>
					<tr>
						<td>
							<div class="flt_right">
								<input type="button" class="btn btn_style kill_btn" value="Kill Process" data-i18n="build.label.killprocess">
								<a href="#" id="copyLog">
									<img src="themes/default/images/helios/buildreport_icon.png" width="15" height="18" border="0" alt="">
								</a>
							</div>
							<div class="flt_left"><span data-i18n="build.label.progress"></span></div>
						</td>
					</tr>
				 </thead>
		        </table>
		        <div class="consolescrolldiv" autoscroll="true">
					<div id="testConsole" class="console_pad">
					</div>
				</div>
		    </div>
	    <!-- Progress div ends here -->
    </section>
