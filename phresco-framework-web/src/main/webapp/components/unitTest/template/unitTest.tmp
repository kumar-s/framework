<section class="optiontitle qual_unit">
	<div class="unit_text">
    	Unit Test
	</div>
    <div class="flt_right">
		<div class="unit_view hideContent">
            <span>
            	<a id="tabularView" href="#"><img src="themes/default/images/helios/quality_table_on.png" width="25" height="25" border="0" alt=""><b>Table View</b></a>
            </span>
            <span>
            	<a id="graphicalView" href="#"><img src="themes/default/images/helios/quality_graph_off.png" width="26" height="24" border="0" alt="">Graph View</a>
            </span>    
        </div>
    	<div class="unit_path">
            <input type="button" value="Open Folder" class="btn btn_style openfolder_icon_btn" id="openFolder">
            <input type="button" value="Copy Path" class="btn btn_style copypath_icon_btn" id="copyPath">
        </div>
            
        {{#if projectModules}}
            <div class="code_report">
            	Module
            </div>
            <div class="navbar navbar-static code_func module">
                <ul class="nav">
                    <li id="fat-menu" class="dropdown">
						<a href="#" id="modulesDrop" role="button" class="dropdown-toggle" data-toggle="dropdown" 
							value="{{#modules projectModules true}}{{/modules}}">{{#modules projectModules true}}{{/modules}}<b class="caret"></b></a>
                      	<div class="dropdown-menu cust_sel code_test_opt" role="menu" aria-labelledby="modulesDrop">
	                      	<ul>
	                      		{{#modules projectModules false}}{{/modules}}
                        	</ul>
						</div>
                    </li>
                </ul>
            </div>
        {{/if}}
    
        {{#if reportOptions}}
            <div class="code_report">
            	Technology
            </div>
            <div class="navbar navbar-static code_func">
                <ul class="nav">
                    <li id="fat-menu" class="dropdown">
						<a href="#" id="reportOptionsDrop" role="button" class="dropdown-toggle" data-toggle="dropdown" 
							value="{{#report reportOptions true}}{{/report}}">{{#report reportOptions true}}{{/report}}<b class="caret"></b></a>
						<div class="dropdown-menu cust_sel code_test_opt" role="menu" aria-labelledby="reportOptionsDrop">
	                      	<ul>
	                      		{{#report reportOptions false}}{{/report}}
	                        </ul>
						</div>
                    </li>
                </ul>
            </div>
        {{/if}}
           
        <div class="code_anal">
        	{{#if userPermissions.manageTests}}
            	<input type="button" value="Test" id="unitTestBtn" class="btn btn_style">
        	{{else}}
        		<input type="button" value="Test" class="btn btn_style" disabled>
        	{{/if}}
                
			<div id="unit_popup" class="dyn_popup" style="display:none;">
				<form id="unitTestForm">
					<table class="table" cellpadding="0" cellspacing="0" border="0">
						<tbody id="dynamicContent"></tbody>
					</table>
				</form>
                <div class="flt_right">
                    <input type="button" value="Test" class="btn btn_style" id="runUnitTest">
                    <input type="button" value="Close" class="btn btn_style dyn_popup_close">
				</div>
			</div>
		</div>
	</div>
</section>
<div class="mainContent" id="testResult"></div>