<section class="content_main code_content testSuiteTable">
	<!-- Testsuites div starts here -->
	<div class="qual_unit_main unit_info" id="testSuites">
		{{#if testSuites}}
	    	<table class="table table-striped table_border table-bordered" cellpadding="0" cellspacing="0" border="0">
				 <thead class="fixedHeader">
	            	<tr>
						<th data-i18n="testresult.label.testsuite"></th>
		              	<th data-i18n="testresult.label.total"></th>
		              	<th data-i18n="testresult.label.success"></th>
		              	<th data-i18n="testresult.label.failure"></th>
		              	<th class="{{#showManualColumn}}{{/showManualColumn}}" data-i18n="testresult.label.error"></th>
		              	<th class="{{#hideManualColumn}}{{/hideManualColumn}}" data-i18n="testresult.label.notApplicable"></th>
		              	<th class="{{#hideManualColumn}}{{/hideManualColumn}}" data-i18n="testresult.label.blocked"></th>
		              	<th class="{{#hideManualColumn}}{{/hideManualColumn}}" data-i18n="testresult.label.notExecuted"></th>
		              	<th class="{{#hideManualColumn}}{{/hideManualColumn}}" data-i18n="testresult.label.testCoverage"></th>
	            	</tr>
	          	</thead>
	          	<tbody class="scrollContent" style="height:475px;">
	          		{{#each testSuites}}
		              	<tr>
		                	<td><a href="#" name="testDescription">{{name}}</a></td>  
		                	<td>{{total}}</td>
		                	<td>{{success}}</td>
		                	<td>{{failures}}</td>
		                	<td class="{{#showManualColumn}}{{/showManualColumn}}">{{errors}}</td>
		                	<td class="{{#hideManualColumn}}{{/hideManualColumn}}">{{notApplicable}}</td>
		                	<td class="{{#hideManualColumn}}{{/hideManualColumn}}">{{blocked}}</td>
		                	<td class="{{#hideManualColumn}}{{/hideManualColumn}}">{{notExecuted}}</td>
		                	<td class="{{#hideManualColumn}}{{/hideManualColumn}}">{{testCoverage}}</td>
		              	</tr>
	              	{{/each}}
	          	</tbody>
	    	</table>
    	{{else}}
			<div class="alert alert-block" style="text-align: center; margin: auto 0;">{{message}}</div>
		{{/if}}
    </div>
    <!-- Testsuites div ends here -->
    
    <!-- Test class of the testsuite div ends here -->
    <div class="qual_unit_main unit_info hideContent" id="testCases">
    	
    	
    	<!-- manual test update dynamic popup on click edit -->
    	<div id="updateManualTestCase_popup" class="dyn_popup" style="display:none">
            <form id="updateManualTestCaseForm">
				<table class="table node_table" cellpadding="0" cellspacing="0" border="0">
					<tbody>
						<tr>
							<td>Test Scenarios<br>
								<input type="text" id="testSuiteName" placeholder="TestSuite Name" name="testSuiteName" value="">
							</td>
							<td>Features<sup>*</sup><br>
								<input type="text" placeholder="Name of the Features" name="featureId" value="">
							</td>
						</tr>
						<tr>
							<td>TestCases<sup>*</sup><br>
								<input type="text" placeholder="Name of the TestCases" name="testCaseId" value="">
							</td>
							<td>Status<br>
								<select name="status">
									<option value="success">Success</option>
									<option value="failure">Failure</option>
								</select>
							</td>
						</tr>
						<tr>
							<td>TestCase Description<br>
								<textarea name="description"></textarea>
							</td>
							<td>Test Steps<br>
								<textarea name="steps"></textarea>
							</td>
						</tr>
						<tr>
							<td>Expected Result<br>
								<textarea name="expectedResult"></textarea>
							</td>
							<td>Actual Result<br>
								<textarea name="actualResult"></textarea>
							</td>
						</tr>
						<tr>
							<td colspan="2">Bug ID/Comments<br>
								<textarea name="bugComment"></textarea>
							</td>
						 </tr>
					</tbody>
				</table>
				<div class="flt_right">
					<input type="button" value="Save" class="btn btn_style" name="addTestCase">
					<input type="button" value="Close" class="btn btn_style dyn_popup_close">
				</div>
			</form>
		</div>
		<!-- manual test update dynamic popup on click edit ends here -->	
				
				
    </div>
    <!-- Test class of the testsuite div ends here -->
    
    <!-- Graphical view div starts here -->
    <div class="qual_unit_main unit_info hideContent" id="graphView">
    	<div class="graph_area">
        	<canvas id="bar" width="620" height="400">[No canvas support]</canvas>
        	<canvas id="pie" width="620" height="400">[No canvas support]</canvas>
        </div>
    </div>
    <!-- Graphical view div ends here -->

    <!-- Progress div starts here-->
    <div class="unit_close">
    	<a href="#" data-flag="true" id="consoleImg">
			<img src="themes/default/images/helios/console_img.png" width="33" height="92" border="0" alt="">
			<div class="progress_loading" style="display:none;"><img src="themes/default/images/helios/progress_load.gif" border="0"></div>
		</a>
	</div>
    <div class="unit_progress">
        <table id="unit_progress" class="table table-striped table_border table-bordered" cellpadding="0" cellspacing="0" border="0">
            <thead>
                <tr>
					<td>
						<div class="flt_right">
							<input type="button" class="btn btn_style kill_btn" value="Kill Process" data-i18n="[value]build.label.killprocess">
							<a href="#" id="buildCopyLog">
								<img src="themes/default/images/helios/buildreport_icon.png" width="15" height="18" border="0" alt="">
							</a>
						</div>
						<div class="flt_left"><span data-i18n="build.label.progress"></span></div>
					</td>
				</tr>
			</thead>
			<tbody class="scrollContent">
            	<tr>
					<td id="testConsole">
					</td>
				</tr>
        	</tbody>
        </table>
    </div>
    <!-- Progress div ends here -->
</section>