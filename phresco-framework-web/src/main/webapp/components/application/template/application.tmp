
    <section class="content_main create_proj" id="appinfo">
          <table class="table table-striped table_border table-bordered" cellpadding="0" cellspacing="0" border="0">  
            <tbody name="layercontents"> 
			{{#each projectlist.appdetails.data.appInfos}}
            	<tr>
                    <td data-i18n="application.edit.name"><sup>*</sup></td>
                    <td><input type="text" name="appName" placeholder="Dr-Drupal7" value="{{name}}"/></td>
                    <td data-i18n="application.edit.code"><sup>*</sup></td>
                    <td><input type="text" placeholder="Application Code" name="appCode" value="{{code}}"/></td>
                    <td class="desc" rowspan="3" data-i18n="application.edit.description"></td>
                    <td class="descdata" rowspan="3"><textarea rows="5" name="appDesc">{{description}}</textarea></td>
					<td colspan="2"></td>
                </tr>
                <tr>
                	<td data-i18n="application.edit.appdirectory"></td>
                    <td><input type="text" placeholder="1.0" name="appDirName" value="{{appDirName}}"/></td>
                    <td data-i18n="application.edit.version"><sup>*</sup></td>
                    <td><input type="text" placeholder="2.0" readonly value="{{version}}" name="appVersion"/></td>
					<td colspan="3"></td>
                </tr>
                <tr>
                    <td data-i18n="application.edit.technology"></td>
                    <td><input type="text" placeholder="Android Hybrid" name="technology" value="{{techInfo.id}}" readonly/>
						<input class="tech_ver" type="text" placeholder="4.0.3" name="techVersion" value="{{techInfo.version}}" readonly/></td>
                    <td data-i18n="application.edit.pilotproject">Pilot Projects</td>
                    <td> Eshop  </td>
					<td colspan="3"></td>
                </tr>
				{{/each}}	  
				
                <tr class="proj_title">
                      <td colspan="3" data-i18n="application.edit.servers"></td>
                      <td colspan="4"><img name="close" id="servers"src="../themes/default/images/helios/close_icon.png"  border="0" alt=""></td>
                    </tr>
					{{#each projectlist.appdetails.data.appInfos}}
						{{#each selectedServers}}
							 <tr class="servers" key="displayed">
							
							 <td data-i18n="application.edit.servers"></td>
							  <td name="servers" class="servers"><select name="appServers" class="appServers">
								<option>Select Server</option>
								{{#each ../../projectlist.serverData}}
									{{{compare id ../artifactGroupId name}}}
								{{/each}}
								  </select>
							  </td>
							  <td data-i18n="application.edit.versions"></td>
							  <td colspan="4" name="version"  class="version">
									<select name="server_version" class="server_version">
										<option>Select Version</option>
										{{#each ../../projectlist.serverData}}
											{{{compareversion id ../artifactGroupId artifactGroup ../artifactInfoIds}}}
										{{/each}}
									</select>
								  <div class="flt_right">
									
									<a href="javascript:;" name="addServer"></a>
									<a href="javascript:;" name="removeServer"><img src="../themes/default/images/helios/minus_icon.png"  border="0" alt=""></a>
								</div>
							  </td>
							
							</tr>
							
						{{/each}}
					{{/each}}
					
					<tr class="servers" key="displayed">
                      <td data-i18n="application.edit.servers"></td>
                      <td name="servers" class="servers"><select name="appServers" class="appServers">
						<option>Select Server</option>
						{{#each projectlist.serverData}}
                            <option value="{{id}}">{{name}}</option>
						{{/each}}
                          </select>
                      </td>
                      <td data-i18n="application.edit.versions"></td>
                      <td colspan="4" name="version" class="version">
							<select name="server_version" class="server_version">
								<option>Select Version</option>
							</select>
						  <div class="flt_right">
							<a href="javascript:;" name="addServer"><img src="../themes/default/images/helios/plus_icon.png" border="0" alt=""> </a> <a href="javascript:;" name="removeServer"></a>
						</div>
                      </td>
                    </tr>
                    
                    <tr class="proj_title">
                      <td colspan="3" data-i18n="application.edit.database"></td>
                      <td colspan="4"><img name="close" id="database" src="../themes/default/images/helios/close_icon.png"  border="0" alt=""></td>
                     </tr>
					 
					{{#each projectlist.appdetails.data.appInfos}}
						{{#each selectedDatabases}}
							  <tr class="database" key="displayed">
							  <td data-i18n="application.edit.database"></td>
							  <td name="servers" class="databases"><select name="databases" class="databases">
								<option>Select Database</option>
								{{#each ../../projectlist.databaseData}}
									{{{compare id ../artifactGroupId name}}}
								{{/each}}
								  </select>
							  </td>
							  <td data-i18n="application.edit.versions"></td>
							  <td colspan="4" name="version" class="version">
									<select name="db_version" class="db_version">
										<option>Select Version</option>
										{{#each ../../projectlist.databaseData}}
											{{{compareversion id ../artifactGroupId artifactGroup ../artifactInfoIds}}}
										{{/each}}										
								  </select>
								   <div class="flt_right">
										<a href="javascript:;" name="addDatabase"></a>
										<a href="javascript:;" name="removeDatabase"><img src="../themes/default/images/helios/minus_icon.png" border="0" alt=""></a>
									</div>
							  </td>
							</tr>
						{{/each}}
					{{/each}}
					
					<tr class="database" key="displayed">
					  <td data-i18n="application.edit.database"></td>
					  <td name="servers" class="databases"><select name="databases" class="databases">
						<option>Select Database</option>
						{{#each projectlist.databaseData}}
							<option value="{{id}}">{{name}}</option>
						{{/each}}
						  </select>
					  </td>
					  <td data-i18n="application.edit.versions"></td>
					  <td colspan="4" name="version" class="version">
							<select name="db_version" class="db_version">
								<option>Select Version</option>
						  </select>
						   <div class="flt_right">
								<a href="javascript:;" name="addDatabase"><img src="../themes/default/images/helios/plus_icon.png" border="0" alt=""></a>
								<a href="javascript:;" name="removeDatabase"></a>
							</div>
					  </td>
					</tr>
						
                    <tr class="proj_title">
                      <td colspan="3" data-i18n="application.edit.webservice"></td>
                      <td colspan="4"><img  name="close" id="webservice" src="../themes/default/images/helios/close_icon.png"  border="0" alt=""></td>
                     </tr>
                     <tr>
                       <td colspan="7">
                          <table width="100%" cellpadding="0" cellspacing="0" border="0">
                          <tr>
                          		<td data-i18n="application.edit.webservice"></td>
									{{#each projectlist.webserviceData}}				
										<td><input type="checkbox" value="{{id}}"> {{name}}</td>
									{{/each}}	
                              	<td></td>
                          </tr>
                          </table>
                      </td>
                      </tr>
					<tr class="proj_title">
                          <td colspan="3">Testing</td>
                          <td colspan="4"><img src="../themes/default/images/helios/close_icon.png" border="0" alt=""></td>
                       </tr>
                       <tr>
                            <td>Functional Framework</td>		
                            <td>
                                <select>
                                    <option>PHPunit</option>
                                    <option>Junit</option>
                                    <option>TestNG</option>
                                    <option>Nunit</option>
                                </select>
                            </td>
                            <td>Tools</td>
                            <td>
                                <select>
                                    <option>Selenium Web Driver</option>
                                    <option>Selenium Grid</option>
                                    <option>Cucumber</option>
                                </select>
                            </td>
                            <td>Version</td>
                            <td>
                                <select>
                                    <option>1.0</option>
                                    <option>2.0</option>
                                    <option>3.0</option>
                                    <option>4.0</option>
                                </select>
                            </td>
                            <td></td>
                      </tr>					  
              	</tbody>
            </table>
    	<!--content_main div ends here -->
    </section>
    <section class="content_end">
		<div class="flt_left">
        	<input type="submit" name="servers" value="Server" class="btn btn_style server_btn" style="display:none;">
            <input type="submit" name="database" value="Database" class="btn btn_style database_btn"  style="display:none;">
            <input type="submit" name="webservice" value="Webservice" class="btn btn_style webservice_btn" style="display:none;">
        </div>
        <div class="flt_right">
			<input type="submit" data-i18n="[value]application.btn.update;" id="updatebutton" class="btn btn_style update_btn">
            <input type="submit" data-i18n="[value]application.btn.cancel;" id="cancelbutton" class="btn btn_style cancel_icon_btn">
        </div>
    	<!--content_title ends here -->
    </section>