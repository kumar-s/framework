
    <section class="content_main create_proj" id="appinfo">
          <table class="table table-striped table_border table-bordered" cellpadding="0" cellspacing="0" border="0">  
            <tbody name="layercontents"> 
			{{#each projectlist.appdetails.data.appInfos}}
            	<tr>
                    <td><span data-i18n="application.edit.name"></span><sup>*</sup></td>
                    <td><input type="text" name="appName" placeholder="Dr-Drupal7" value="{{name}}"/></td>
                    <td><span data-i18n="application.edit.code"></span><sup>*</sup></td>
                    <td><input type="text" placeholder="Application Code" name="appCode" value="{{code}}"/></td>
                    <td class="desc" rowspan="3" data-i18n="application.edit.description"></td>
                    <td class="descdata" rowspan="3"><textarea rows="5" name="appDesc" class="appDesc" id="appDesc">{{description}}</textarea></td>
					<td colspan="2"></td>
                </tr>
                <tr>
                	<td><span data-i18n="application.edit.appdirectory"></span><sup>*</sup></td>
                    <td><input type="text" placeholder="1.0" name="appDirName" value="{{appDirName}}"/></td>
                    <td data-i18n="application.edit.version"><sup>*</sup></td>
                    <td><input type="text" placeholder="2.0" readonly value="{{version}}" name="appVersion"/></td>
					<td colspan="3"></td>
                </tr>
                <tr>
                    <td data-i18n="application.edit.technology"></td>
                    <td><input type="text" placeholder="Android Hybrid" name="technology" value="{{techInfo.name}}" readonly/>
						<input class="tech_ver" type="text" placeholder="4.0.3" name="techVersion" value="{{techInfo.version}}" readonly size="5"/></td>
                    <td></td>
                    <td>   </td>
					<td colspan="3"></td>
                </tr>
				{{/each}}	  
				
                <tr class="proj_title">
                      <td colspan="3" data-i18n="application.edit.servers"></td>
                      <td colspan="4"><img name="close" id="servers"src="themes/default/images/helios/close_icon.png"  border="0" alt=""></td>
                    </tr>
					{{#each projectlist.appdetails.data.appInfos}}
						{{#each selectedServers}}
							 <tr class="servers" key="displayed">
							
							 <td><span data-i18n="application.edit.servers"></span>&nbsp;<span class="paid">{{position @index}}</span></td>
							  <td name="servers" class="servers"><select name="appServers" class="appServers">
								<option value=0>Select Server</option>
								{{#each ../../projectlist.serverData}}
									{{{compare id ../artifactGroupId name}}}
								{{/each}}
								  </select>
							  </td>
							  <td data-i18n="application.edit.versions"></td>
							  <td colspan="4" name="version"  class="version">
									<select name="server_version" class="server_version selectpicker" title="Select Version" multiple data-selected-text-format="count>3">
										{{#each ../../projectlist.serverData}}
											{{{compareversion id ../artifactGroupId artifactGroup ../artifactInfoIds}}}
										{{/each}}
									</select>
								  <div class="flt_right">
									<a href="javascript:;" name="addServer">
										{{#isLastRow ../selectedServers @index}}
											<img src="themes/default/images/helios/plus_icon.png" border="0" alt=""> 
										{{/isLastRow}}
									</a>
									<a href="javascript:;" name="removeServer">
										{{#isNotSingleRow ../selectedServers}}
											<img src="themes/default/images/helios/minus_icon.png"  border="0" alt="">
										{{/isNotSingleRow}}	
									</a>
								</div>
							  </td>
							
							</tr>
							
						{{/each}}
					{{/each}}
					
					{{#unless projectlist.appdetails.data.appInfos.0.selectedServers}}
						<tr class="servers" key="displayed">
						  <td><span data-i18n="application.edit.servers"></span>&nbsp;<span class="paid">{{positionouter projectlist.appdetails.data.appInfos}}</span></td>
						  <td name="servers" class="servers"><select name="appServers" class="appServers">
							<option value=0>Select Server</option>
							{{#each projectlist.serverData}}
								<option value="{{id}}">{{name}}</option>
							{{/each}}
							  </select>
						  </td>
						  <td data-i18n="application.edit.versions"></td>
						  <td colspan="4" name="version" class="version">
								<select name="server_version" class="server_version selectpicker" title="Select Version" multiple data-selected-text-format="count>3">
								<option value=0>Select Version</option>
								</select>
							  <div class="flt_right">
								<a href="javascript:;" name="addServer"><img src="themes/default/images/helios/plus_icon.png" border="0" alt=""> </a> <a href="javascript:;" name="removeServer"></a>
							</div>
						  </td>
						</tr>
                     {{/unless}}
					 
                    <tr class="proj_title">
                      <td colspan="3" data-i18n="application.edit.database"></td>
                      <td colspan="4"><img name="close" id="database" src="themes/default/images/helios/close_icon.png"  border="0" alt=""></td>
                     </tr>

					{{#each projectlist.appdetails.data.appInfos}}
						{{#each selectedDatabases}}
							  <tr class="database" key="displayed">
							  <td><span data-i18n="application.edit.database"></span>&nbsp;<span class="paid">{{position @index}}</span></td>
							  <td name="servers" class="databases"><select name="databases" class="databases">
								<option value=0>Select Database</option>
								{{#each ../../projectlist.databaseData}}
									{{{compare id ../artifactGroupId name}}}
								{{/each}}
								  </select>
							  </td>
							  <td data-i18n="application.edit.versions"></td>
							  <td colspan="4" name="version" class="version">
									<select name="db_version" class="db_version selectpicker" title="Select Version" multiple data-selected-text-format="count>3">
										{{#each ../../projectlist.databaseData}}
											{{{compareversion id ../artifactGroupId artifactGroup ../artifactInfoIds}}}
										{{/each}}										
								  </select>
								   <div class="flt_right">
										<a href="javascript:;" name="addDatabase">
											{{#isLastRow ../selectedDatabases @index}}
												<img src="themes/default/images/helios/plus_icon.png" border="0" alt="">
											{{/isLastRow}}
										</a>
										<a href="javascript:;" name="removeDatabase">
											{{#isNotSingleRow ../selectedDatabases}}
												<img src="themes/default/images/helios/minus_icon.png"  border="0" alt="">
											{{/isNotSingleRow}}	
										</a>
									</div>
							  </td>
							</tr>
						{{/each}}
					{{/each}}
					
					{{#unless projectlist.appdetails.data.appInfos.0.selectedDatabases}}
						<tr class="database" key="displayed">
						  <td><span data-i18n="application.edit.database"></span>&nbsp;<span class="paid">{{dbouter projectlist.appdetails.data.appInfos}}</span></td>
						  <td name="servers" class="databases"><select name="databases" class="databases">
							<option value=0>Select Database</option>
							{{#each projectlist.databaseData}}
								<option value="{{id}}">{{name}}</option>
							{{/each}}
							  </select>
						  </td>
						  <td data-i18n="application.edit.versions"></td>
						  <td colspan="4" name="version" class="version">
								<select name="db_version" class="db_version selectpicker" title="Select Version" multiple data-selected-text-format="count>3">
								<option value=0>Select Version</option>
							 	</select>
							   <div class="flt_right">
									<a href="javascript:;" name="addDatabase"><img src="themes/default/images/helios/plus_icon.png" border="0" alt=""></a>
									<a href="javascript:;" name="removeDatabase"></a>
								</div>
						  </td>
						</tr>
					{{/unless}}
						
                    <tr class="proj_title">
                      <td colspan="3" data-i18n="application.edit.webservice"></td>
                      <td colspan="4"><img  name="close" id="webservice" src="themes/default/images/helios/close_icon.png"  border="0" alt=""></td>
                     </tr>
                     <tr class="webservice" key="displayed">
                       <td colspan="7">
                          <table width="100%" cellpadding="0" cellspacing="0" border="0">
                          <tr>
                          		<td data-i18n="application.edit.webservice"></td>
									{{#each projectlist.webserviceData}}
										{{#isSelectedWebService ../projectlist.appdetails.data.appInfos id}}
											<td><input class="webservice_chkbox" checked type="checkbox" value="{{id}}"> {{name}}</td>
										{{else}}
											<td><input class="webservice_chkbox" type="checkbox" value="{{id}}"> {{name}}</td>
										{{/isSelectedWebService}}
									{{/each}}	
                              	<td></td>
                          </tr>
                          </table>
                      </td>
                      </tr>
					<tr class="proj_title">
                          <td colspan="3">Testing</td>
                          <td colspan="4"><img name="close" id="testing" src="themes/default/images/helios/close_icon.png" border="0" alt=""></td>
                       </tr>
                       <tr>
                            <td>Functional Framework</td>		
                            <td>
                                <select>
                                    <option>PHPunit</option>
                                    <option>Junit</option>
                                    <option>TestNG</option>
                                    <option>Nunit</option>
                                </select>
                            </td>
                            <td>Tools</td>
                            <td>
                                <select>
                                    <option>Selenium Web Driver</option>
                                    <option>Selenium Grid</option>
                                    <option>Cucumber</option>
                                </select>
                            </td>
                            <td>Version</td>
                            <td>
                                <select>
                                    <option>1.0</option>
                                    <option>2.0</option>
                                    <option>3.0</option>
                                    <option>4.0</option>
                                </select>
                            </td>
                            <td></td>
                      </tr>					  
              	</tbody>
            </table>
    	<!--content_main div ends here -->
    </section>
    <section class="content_end">
		<div class="flt_left">
        	<input type="submit" name="servers" value="Server" class="btn btn_style server_btn" style="display:none;">
            <input type="submit" name="database" value="Database" class="btn btn_style database_btn"  style="display:none;">
            <input type="submit" name="webservice" value="Webservice" class="btn btn_style webservice_btn" style="display:none;">
            <input type="submit" name="testing" value="Testing" class="btn btn_style testing_btn" style="display:none;">
        </div>
        <div class="flt_right">
        	{{#if userPermissions.manageApplication}}
				<input type="submit" data-i18n="[value]application.btn.update;" id="updatebutton" class="btn btn_style update_btn">
			{{else}}
				<input type="submit" data-i18n="[value]application.btn.update;" id="updatebutton" class="btn btn_style update_btn" disabled>
			{{/if}}
            <input type="submit" data-i18n="[value]application.btn.cancel;" id="cancelbutton" class="btn btn_style cancel_icon_btn">
        </div>
		<div class="flt_right appSuccessMsg">
			{{getSuccessMsg projectlist.appdetails.data.appInfos.0.appDirName}}
		</div>
    	<!--content_title ends here -->
    </section>