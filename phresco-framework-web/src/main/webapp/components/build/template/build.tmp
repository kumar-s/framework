 <section class="optiontitle buildtitle">
		<div class="flt_left">
        	<input type="button" data-i18n="[value]build.label.openfolder" class="btn btn_style openfolder_icon_btn" id="openFolder">
			<input type="button" data-i18n="[value]build.label.copypath" class="btn btn_style copypath_icon_btn" id="copyPath">
        </div>
        <div class="alert_div" style="display:none;" data-i18n="build.label.ProgressMsg"></div>
        
        <div class="flt_right">
            <div class="build_genbuild">
			{{#if userPermissions.manageBuilds}}
				<input type="submit" data-i18n="[value]build.label.runagainstsource" name="build_runagsource" class="btn btn_style">
			{{else}}
				<input type="submit" data-i18n="[value]build.label.runagainstsource" name="build_runagsource" class="btn btn_style" disabled>
			{{/if}}	
			  <form name="runAgainstForm">
				  <div id="build_runagsource" class="dyn_popup dynamic_cont" style="display:none;width:310px;">
					<ul class="row dynamicControls">
					</ul>
					<div class="dynamicPopupLoading" style="display:none;">&nbsp</div>
					<div class="hiddenControls"></div>	
					 <div class="flt_right">
						<input id="runSource" type="button" data-i18n="[value]build.label.run" class="btn btn_style">
						<input type="button" data-i18n="[value]build.label.close" class="btn btn_style dyn_popup_close">
					 </div>
				  </div>
				</form>  
             </div>
			<input id="stop" type="submit" data-i18n="[value]build.label.stop" class="btn btn_style_off">
			<input id="restart" type="submit" data-i18n="[value]build.label.restart" class="btn btn_style_off">
            <div class="build_genbuild">
				{{#if userPermissions.manageBuilds}}
                	<input type="submit" data-i18n="[value]build.label.generatebuild" name="build_genbuild" class="btn btn_style">
				{{else}}	
					<input type="submit" data-i18n="[value]build.label.generatebuild" name="build_genbuild" class="btn btn_style" disabled>
				{{/if}}
				<form name="buildForm">
                    <div id="build_genbuild" class="dyn_popup dynamic_cont" style="display:none;width:310px;">
                        <ul class="row dynamicControls">
                        </ul>
                        <div class="dynamicPopupLoading" style="display:none;">&nbsp</div>
						<div class="hiddenControls"></div>
                        <div class="flt_right">
                            <input id="buildRun" type="button" value="Build" class="btn btn_style dyn_popup_close">
                            <input type="button" value="Close" class="btn btn_style dyn_popup_close">
                        </div>
                    </div>
                </form>
             </div>
             <div class="build_minifier">
				{{#if userPermissions.manageBuilds}}	
					<input id="minifier" type="submit" data-i18n="[value]build.label.minifer"  name="build_minifier" class="btn btn_style">
				{{else}}	
					<input id="minifier" type="submit" data-i18n="[value]build.label.minifer"  name="build_minifier" class="btn btn_style" disabled>
				{{/if}}
					  <div id="build_minifier" class="dyn_popup" style="display:none;">
                        <table class="table table-striped table_border table-bordered" cellpadding="0" cellspacing="0" border="0">
                            <thead>
                                <tr>
                                	<th colspan="2">JS Minification</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td><input type="text"></td>
                                    <td><input type="text"><input type="button" data-i18n="[value]build.label.browse" class="btn btn_style"><a href="#"><img src="themes/default/images/helios/plus_icon.png" alt=""></a></td>
                                </tr>
                            </tbody>
                          </table>
                          <table class="table table-striped table_border table-bordered" cellpadding="0" cellspacing="0" border="0">
                            <thead>
                                <tr>
                                	<th colspan="2">CSS Minification</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td><input type="text"></td>
                                    <td><input type="text"><input type="button" data-i18n="[value]build.label.browse" class="btn btn_style"><a href="#"><img src="themes/default/images/helios/plus_icon.png" alt=""></a></td>
                                </tr>
                            </tbody>
                          </table>
                         <div class="flt_right">
                            <input type="button" data-i18n="[value]build.label.minify" class="btn btn_style dyn_popup_close">
                            <input type="button" data-i18n="[value]build.label.close" class="btn btn_style dyn_popup_close">
                         </div>
                      </div>
             </div>
             <!--flt_right div ends here -->
        </div>    
    </section>
	
	<section class="content_main code_content testSuiteTable">
		<div class="qual_unit_main unit_info">
			{{#if buildInfos}}
				<table class="table table-striped table_border table-bordered big" cellpadding="0" cellspacing="0" border="0" id="buildRow">
					<thead class="fixedHeader">
						<tr>
							<th data-i18n="build.label.bNo"></th>
							<th data-i18n="build.label.date"></th>
							<th data-i18n="build.label.download"></th>
							<th data-i18n="build.label.deploy"></th>
							<th data-i18n="build.label.delete"></th>
						</tr>    
					</thead>  
					<tbody class="scrollContent"> 
						{{#each buildInfos}}
							<tr>
								<td name="{{buildNo}}">{{buildNo}}</td>
								<td>{{timeStamp}}</td>
								<td class="list_img">
									<a href="#">
										<img name="downloadBuild" src="themes/default/images/helios/download_icon.png" width="15" height="18" border="0" alt="">
									</a>
								</td>
								<td class="list_img">
									<a href="#">
										{{#if ../userPermissions.manageBuilds}}
											<img name="deployBuild" src="themes/default/images/helios/deploy_icon.png" width="16" height="20" border="0" alt="">
										{{else}}
											<img src="themes/default/images/helios/deploy_icon_off.png" width="16" height="20" border="0" alt="">
										{{/if}}
									</a>
									<div id="deploye_{{buildNo}}" class="dyn_popup popup_bg" style="display:none;">
										<div id="bdeploy_{{buildNo}}">
											<form name="deployForm">
												<ul class="row dynamicControls">
												</ul>
												<input type="hidden" name="buildNumber" value="{{buildNo}}" />
											</form>
											<div class="flt_right">
												<input type="button" name="deploy" data-i18n="[value]build.label.deploy" class="btn btn_style dyn_popup_close">
												<input type="button" data-i18n="[value]build.label.close" class="btn btn_style dyn_popup_close">
											</div>
										</div>
									</div>
								</td>
								<td class="list_img">
									{{#if ../userPermissions.manageBuilds}}
										<a name="delete_{{buildNo}}" class="tooltiptop" title="" data-placement="top" data-toggle="tooltip" href="#" data-original-title="Delete Row">
											<img name="deleteBuild" src="themes/default/images/helios/delete_icon.png" width="16" height="20" border="0" alt="">
										</a>
										<div id="delete_{{buildNo}}" class="dyn_popup">
											<div data-i18n="build.label.deleteConform"></div>
											<div>
												<input type="button" name="buildDelete" data-i18n="[value]build.label.yes" class="btn btn_style dyn_popup_close" />
												<input type="button" data-i18n="[value]build.label.no" class="btn btn_style dyn_popup_close" />
											</div>
										</div>
									{{else}}
										<img src="themes/default/images/helios/delete_row_off.png" width="16" height="20" border="0" alt="">
									{{/if}}
								</td>
							</tr>
						{{/each}}
					</tbody>  
				</table>
			{{else}}
				<div class="alert" style="text-align: center;" data-i18n="build.label.nodata"></div>
			{{/if}}	
		</div>
		
		<div class="unit_close">
			<a href="#" data-flag="true" id="buildConsole">
				<img src="themes/default/images/helios/console_img.png" width="33" height="92" border="0" alt="">
				<div class="progress_loading" style="display:none;"><img src="themes/default/images/helios/progress_load.gif" border="0"></div>
			</a>
		</div>
		
		<div class="unit_progress build_progress">
			<table id="build_progress" class="table table-striped table_border table-bordered" cellpadding="0" cellspacing="0" border="0">
				<thead>
					<tr>
						<td>
							<div class="flt_right">
								<input type="button" class="btn btn_style kill_btn" data-i18n="[value]build.label.kill">
								<a href="#" id="buildCopyLog">
									<img src="themes/default/images/helios/buildreport_icon.png" width="15" height="18" border="0" alt="">
								</a>
							</div>
							<div class="flt_left" data-i18n="build.label.progress"></div>
						</td>
					</tr>
				 </thead>
				 <tbody class="scrollContent">
					<tr>
						<td id="logContent">
						</td>
					</tr>
				</tbody>
			</table>
		</div>
	</section>