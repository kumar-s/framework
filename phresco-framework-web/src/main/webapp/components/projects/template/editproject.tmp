<section id="addproject">   
	
     <section class="content_main create_proj">
          <table class="table table-striped table_border table-bordered create_edit_table" cellpadding="0" cellspacing="0" border="0">  
            <tbody> 
				{{#editProject}}
            	<tr>
                    <td data-i18n="project.create.label.name"><sup>*</sup></td>
                    <td><input type="text" value="{{name}}" placeholder="Project Name"/></td>
                    <td data-i18n="project.create.label.code"><sup>*</sup></td>
                    <td><input type="text" value="{{projectCode}}" placeholder="Project Code"/></td>
                    <td data-i18n="project.create.label.version"></td>
                    <td><input type="text" value="{{version}}"/></td>
                     <td data-i18n="project.create.label.description"></td>
                    <td><input type="text" value="{{description}}" placeholder="Description"/></td>
                </tr>
                <tr>
                    <td data-i18n="project.create.label.startdate"></td>
                    <td class="startdate"><input type="text"><img src="../themes/default/images/helios/cal_icon.png"  width="25" height="20" border="0" alt=""></td>
                    <td data-i18n="project.create.label.enddate"></td>
                    <td class="enddate"><input type="text"><img src="../themes/default/images/helios/cal_icon.png"  width="25" height="20" border="0" alt=""></td>
                    <td colspan="2">
                        <select class="custon_dropdown">
                            <option>Pre Built</option>
                            <option>Custom</option>
                        </select>
                        <select class="custon_dropdown">
                            <option>Select</option>
                        </select>
                    </td>
                    <td data-i18n="project.create.label.pilotprojects"></td>
                    <td>
                        <select>
                            <option>Select Pilot Projects</option>
                            <option>Select Pilot Projects</option>
                            <option>Select Pilot Projects</option>
                        </select>
                    </td>
                </tr>
			{{/editProject}}
               <tr id="appLayaer" class="proj_title">
                  <td colspan="4">{{applicationlayerData.name}}</td>
                  <td colspan="4"><img name="close" width="25" height="20" border="0" alt=""></td>
                </tr>
                <tr class="applnLayer">
                  <td data-i18n="project.create.label.appcode"></td>	
                  <td><input type="text"></td>
                  <td data-i18n="project.create.label.technology"></td>
                  <td name="technology">
				      <select name="appln_technology">
                        <option>Select Technology</option>
						{{#each applicationlayerData.techGroups}}
							{{#each techInfos}}
							  <option value="{{id}}">{{name}}</option>
							{{/each}}	
						{{/each}}
                      </select>
				  </td>
                  <td data-i18n="project.create.label.applicationversion"></td>
                  <td colspan="3" name="version">
                    <select name="appln_version">
                        <option>Select Version</option>
                    </select>
                    <div class="flt_right">
					  <a href="javascript:;"><img name="addApplnLayer" src="../themes/default/images/helios/plus_icon.png" width="25" height="20" border="0" alt=""></a>
					  <a href="javascript:;"><img name="removeApplnLayer" width="25" height="20" border="0" alt=""></a>
				    </div>
                  </td>
                </tr>
                     <tr id="webLayaer" class="proj_title">
                      <td colspan="4">{{weblayerData.name}}</td>
                      <td colspan="4"><img name="close" width="25" height="20" border="0" alt=""></td>
                     </tr>
                     <tr class="webLayer">
                      <td data-i18n="project.create.label.appcode"></td>	
	                  <td><input type="text"></td>
                      <td data-i18n="project.create.label.weblayer"></td>	
                      <td name="web">
                      	  <select name="weblayer">
                            <option>Select Web Layer</option>
							{{#each weblayerData.techGroups}}
								<option value="{{id}}">{{name}}</option>
							{{/each}}
                          </select>
                          <span class="loading_bar" style="display:none;"><img src="../themes/default/images/helios/loading.gif" width="16" height="16" border="0"></span>
                      </td>
                      <td data-i18n="project.create.label.widget"></td>
                      <td name="widget">
                      	 <select name="web_widget">
                            <option>Select Widget</option>
                         </select>
                      </td>
                      <td data-i18n="project.create.label.widgetversion"></td>
                      <td name="widgetversion">
                      	<select name="web_version">
                            <option>Select Version</option>
                        </select>
                        <div class="flt_right">
							<a href="javascript:;"><img name="addWebLayer" src="../themes/default/images/helios/plus_icon.png" width="25" height="20" border="0" alt=""> </a>
							<a href="javascript:;"><img name="removeWebLayer" width="25" height="20" border="0" alt=""></a>
						</div>
                       </td>
                      </tr>
    
                    <tr id="mobLayaer" class="proj_title">
                      <td colspan="4">{{mobilelayerData.name}}</td>
                      <td colspan="4"><img name="close"  width="25" height="20" border="0" alt=""></td>
                     </tr>
                     <tr>
                      <td colspan="8">
                          <table width="100%" cellpadding="0" cellspacing="0" border="0">
                          <tr class="mobileLayer">
                          		<td data-i18n="project.create.label.appcode"></td>	
				                <td><input type="text"></td>
                                <td data-i18n="project.create.label.mobile"></td>		
                                <td name="mobile">
                                	<select name="mobile_layer">
                                        <option>Select Model</option>
										{{#each mobilelayerData.techGroups}}
											<option value="{{id}}">{{name}}</option>
										{{/each}}
                                    </select>
                                </td>
                                <td data-i18n="project.create.label.types"></td>
                                <td name="types">      
                                    <select name="mobile_types">
                                        <option>Select Type</option>
                                    </select>
		                        </td>
                                <td data-i18n="project.create.label.mobileversion"></td>
                                <td name="mobileversion">      
                                    <select name="mobile_version">
                                        <option>Select Version</option>
                                    </select>
		                        </td>
                                <td>
                                	  <input type="checkbox"> <font data-i18n="project.create.label.mobile"></font> &nbsp;&nbsp;&nbsp;<input type="checkbox"> <font data-i18n="project.create.label.tablet"></font>&nbsp;&nbsp;&nbsp;
                                </td>
                                <td>
                                	<div class="flt_right">
										<a href="javascript:;"><img name="addMobileLayer" src="../themes/default/images/helios/plus_icon.png"  width="25" height="20" border="0" alt=""></a> 
										<a href="javascript:;"><img name="removeMobileLayer" width="25" height="20" border="0" alt=""></a>
									</div> 
                                </td>
                          </tr>
                          </table>
                      </td>	
                    </tr>
              	</tbody>
            </table>
    	<!--content_main div ends here -->
    </section>
    <section class="content_end">
        <div class="flt_right">
        	<input type="submit" value="Update" class="btn btn_style add_icon_btn">
            <input type="submit" value="Cancel" class="btn btn_style cancel_icon_btn">
        </div>
    	<!--content_title ends here -->
    </section>
</section>	