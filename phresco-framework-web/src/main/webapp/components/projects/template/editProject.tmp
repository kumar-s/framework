<section id="addproject">   
	
     <section class="content_main create_proj">
          <table class="table table-striped table_border table-bordered create_edit_table" cellpadding="0" cellspacing="0" border="0">  
            <tbody name="layercontents" class="scrollContent"> 
				{{#editProject}}
            	<tr>
                    <td data-i18n="project.create.label.name"><sup>*</sup></td>
                    <td><input type="text" value="{{name}}" name="projectname" placeholder="Project Name" disabled/></td>
                    <td data-i18n="project.create.label.code"><sup>*</sup></td>
                    <td><input type="text" value="{{projectCode}}" name="projectcode" placeholder="Project Code" disabled/></td>
                    <td data-i18n="project.create.label.version"></td>
                    <td><input type="text" name="projectversion" value="{{version}}" disabled/></td>
                    <td data-i18n="project.create.label.description"></td>                              
                    <td rowspan="2"><textarea class="popuptextarea" name="projectdescription"  placeholder="Description" disabled >{{description}}</textarea></td>
                </tr>
                <tr>
					<td colspan="2">
                        <select class="custon_dropdown" disabled>
                            <option>Pre Built</option>
                            <option>Custom</option>
                        </select>
                    </td>
					
                    <td data-i18n="project.create.label.startdate" name="startdate-lbl"></td>
                    <td class="startdate" name="startdateholder"><input name="startdate" type="text" value="{{startDate}}" id="startDate" disabled><span class="add-on"><img src="themes/default/images/helios/cal_icon.png" border="0" alt=""></span></td>
                    <td data-i18n="project.create.label.enddate" name="enddate-lbl"></td>
                    <td colspan="2" class="enddate" name="enddateholder"><input name="enddate" value="{{endDate}}" id="endDate" type="text" disabled><span class="add-on"><span class="add-on"><img class="cal_img1" src="themes/default/images/helios/cal_icon.png" border="0" alt=""></span></span></td>
                </tr>
				<tr>
					{{#if editProject.multiModule}}
                	   <td colspan="2"><input type="checkbox" name="multimodule" value="true" checked><b>&nbsp; Multi Module</b></td>
					   <td><span name="artifactId-lbl" data-i18n="project.create.label.artifactId"></span></td>
					   <td><input name="artifactId" type="text" placeholder="Artifact ID"></td>
					{{else}}
						<td colspan="2"><input type="checkbox" name="multimodule" value="false"><b>&nbsp; Multi Module</b></td>
						<td><span name="artifactId-lbl" data-i18n="project.create.label.artifactId" style="display:none;"></span></td>
					   <td><input name="artifactId" type="text" placeholder="Artifact ID" style="display:none;"></td>
					{{/if}}   
                    <td colspan="6"></td>
                </tr>
			{{/editProject}}
               <tr id="appLayaer" class="proj_title">
                  <td colspan="4">{{applicationlayerData.name}}</td>
                  <td colspan="4"><img name="close" border="0" alt=""><img name="close" id="appLayaer" src="themes/default/images/helios/close_icon.png"  border="0" alt=""></td>
                </tr>
                <tr class="applnLayer" id="applnlayer" key="displayed">
						<td colspan="8" class="appln">
							<table class="table table-striped table_border table-bordered applnlayer" cellpadding="0" cellspacing="0" border="0">
								<thead>
									<tr>
										<th><span data-i18n="project.create.label.appcode"></span><sup>*</sup></th>
										<th><span data-i18n="project.create.label.technology"></span></th>
										<th><span data-i18n="project.create.label.applicationversion"></span></th>
										<th></th>
										<th></th>
										<th colspan="3"><span name="appdependency" data-i18n="project.create.label.dependency"></span></th>
									</tr>
								</thead>
								<tbody class="applnlayer">
									<tr class="applnlayercontent" name="staticApplnLayer" style="display:none;">
										<td class="applnappcode"><input type="text" id="appcode" class="appln-appcode"></td>
										<td name="technology" class="technology">
											<select name="appln_technology" class="appln_technology">
											   <option>Select Technology</option>
											   {{#each applicationlayerData.techGroups}}
													{{#each techInfos}}
													  <option value="{{id}}">{{name}}</option>
													{{/each}}	
												{{/each}}
											</select>
										 </td>
										 <td colspan="3" name="version" class="version">
											<select name="appln_version" class="appln_version">
											   <option>Select Version</option>
											</select>
										 </td>
										 <td colspan="2" class="appdependencyTd">
											<select class="selectpicker appdependencySelect" name="appdependencySelect" multiple title="Select Dependency" data-selected-text-format="count>2">
											   <option value="0" disabled>Select Dependency</option>
											</select>
										 </td>
										 <td>
											<div class="flt_right icon_center"><a name="addApplnLayer"><img src="themes/default/images/helios/plus_icon.png" border="0" alt=""></a><a href="javascript:;" name="removeApplnLayer"></a></div>
										 </td>
								    </tr>
								</tbody>
                           </table>
						</td>
					</tr>
					 <tr id="webLayers" class="proj_title">
                      <td colspan="4">{{weblayerData.name}}</td>
                      <td colspan="4"><img name="close" id="webLayers" src="themes/default/images/helios/close_icon.png" border="0" alt=""></td>
                     </tr>
                     <tr class="webLayer" id="WebLayer" key="displayed">
						  <td colspan="8" class="WebLayer">
							<table class="table table-striped table_border table-bordered WebLayer" cellpadding="0" cellspacing="0" border="0">
								<thead>
									<tr>
										<th><span data-i18n="project.create.label.appcode"></span><sup>*</sup></th>
										<th><span data-i18n="project.create.label.weblayer"></span></td>
										<th><span data-i18n="project.create.label.widget"></span></td>
										<th><span data-i18n="project.create.label.widgetversion"></span></td>
										<th><span name="webdependency" data-i18n="project.create.label.dependency"></span></th>
										<th></th>
									</tr>
								</thead>
								<tbody class="WebLayer">
									<tr class="weblayercontent" name="staticWebLayer" style="display:none;">
										<td class="webappcode"><input type="text" id="webappcode" class="web-appcode"></td>
										<td name="web" class="web">
											<select name="weblayer" class="weblayer">
												<option>Select Layer</option>
												{{#each weblayerData.techGroups}}
													<option value="{{id}}">{{name}}</option>
												{{/each}}
											</select>
										 </td>
										 <td name="widget" class="widget">
											<select name="web_widget" class="web_widget">
												<option>Select Widget</option>
											 </select>
										 </td>
										 <td name="widgetversion" class="widgetversion">
											<select name="web_version" class="web_version">
												<option>Select Version</option>
											</select>
										 </td>
										 <td class="webdependencyTd">
											<select class="selectpicker webdependencySelect" name="webdependencySelect" multiple data-selected-text-format="count>2" title="Select Dependency">
											   <option disabled>Select Dependency</option>
											</select>
										 </td>
										 <td>
											<div class="flt_right icon_center"><a href="javascript:;" name="addWebLayer"><img src="themes/default/images/helios/plus_icon.png" border="0" alt=""></a><a href="javascript:;" name="removeWebLayer"></a></div>
										 </td>
									</tr>
								</tbody>
                           </table>
                        </td>	
                      </tr>
					  <tr id="mobLayers" class="proj_title">
                      <td colspan="4">{{mobilelayerData.name}}</td>
                      <td colspan="4"><img name="close" id="mobLayers" src="themes/default/images/helios/close_icon.png"  border="0" alt=""></td>
                     </tr>
                     <tr class="mobLayer" id="MobLayer" key="displayed">
                      	<td colspan="8" class="mob">
							<table class="table table-striped table_border table-bordered mob-table" cellpadding="0" cellspacing="0" border="0">
								<thead>
									<tr>
										<th><span data-i18n="project.create.label.appcode"></span><sup>*</sup></th>
										<th><span data-i18n="project.create.label.mobile"></span></th>
										<th><span data-i18n="project.create.label.types"></span></th>
										<th><span data-i18n="project.create.label.mobileversion"></span></th>
										<th></th>
										<th></th>
									</tr>
								</thead>
								<tbody class="MobLayer">
									<tr class="mobilelayercontent" name="staticMobileLayer" style="display:none;">
										<td class="mobileappcode"><input type="text" id="mobileappcode" class="mobile-appcode"></td>
										<td name="mobile" class="mobile">
											<select name="mobile_layer" class="mobile_layer">
											   <option>Select Model</option>
											   {{#each mobilelayerData.techGroups}}
												  <option value="{{id}}">{{name}}</option>
											   {{/each}}
											</select>
										 </td>
										 <td name="types" class="types">
											<select name="mobile_types" class="mobile_types">
											   <option>Select Type</option>
											</select>
										 </td>
										 <td colspan="2" name="mobileversion" class="mobileversion">
											<select name="mobile_version" class="mobile_version">
											   <option>Select Version</option>
											</select>
										 </td>
										 <td>
											<div class="flt_right icon_center"><a href="javascript:;" name="addMobileLayer"><img src="themes/default/images/helios/plus_icon.png" border="0" alt=""></a><a href="javascript:;" name="removeMobileLayer"></a> </div>
										 </td>
									</tr>
								</tbody>
                           </table>
                        </td>
                     </tr>
              	</tbody>
            </table>
    	<!--content_main div ends here -->
    </section>
    <section class="content_end">
		<div class="flt_left">
        	<input type="submit" name="appLayaer" value="Application Layer" class="btn btn_style app_layer_icon_btn" style="display:none;">
            <input type="submit" name="webLayers" value="Web Layer" class="btn btn_style web_layer_icon_btn" style="display:none;">
            <input type="submit" name="mobLayers" value="Mobile Layer" class="btn btn_style mobile_layer_icon_btn" style="display:none;">
        </div>
        <div class="flt_right">
        	{{#if userPermissions.manageApplication}}
        		<input type="submit" value="Update" id="updateProject" class="btn btn_style add_icon_btn">
        	{{else}}
        		<input type="submit" value="Update" id="updateProject" class="btn btn_style add_icon_btn" disabled>
    		{{/if}}
            <input type="submit" value="Cancel" id="cancelUpdate" class="btn btn_style cancel_icon_btn">
        </div>
    	<!--content_title ends here -->
    </section>
</section>	