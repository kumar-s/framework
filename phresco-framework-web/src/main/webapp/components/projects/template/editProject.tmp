<section id="addproject">   
	
     <section class="content_main create_proj">
          <table class="table table-striped table_border table-bordered create_edit_table" cellpadding="0" cellspacing="0" border="0">  
            <tbody name="layercontents" class="scrollContent"> 
				{{#editProject}}
            	<tr>
                    <td data-i18n="project.create.label.name"><sup>*</sup></td>
                    <td><input type="text" value="{{name}}" name="projectname" placeholder="Project Name" disabled/></td>
                    <td data-i18n="project.create.label.code"><sup>*</sup></td>
                    <td><input type="text" value="{{projectCode}}" name="projectcode" placeholder="Project Code" disabled/></td>
                    <td data-i18n="project.create.label.version"></td>
                    <td><input type="text" name="projectversion" value="{{version}}" disabled/></td>
                    <td data-i18n="project.create.label.description"></td>                              
                    <td rowspan="2"><textarea class="popuptextarea" name="projectdescription"  placeholder="Description" disabled >{{description}}</textarea></td>
                </tr>
                <tr>
					<td colspan="2">
                        <select class="custon_dropdown" disabled>
                            <option>Pre Built</option>
                            <option>Custom</option>
                        </select>
                    </td>
					
                    <td data-i18n="project.create.label.startdate" name="startdate-lbl"></td>
                    <td class="startdate" name="startdateholder"><input name="startdate" type="text" value="{{startDate}}" id="startDate" disabled><span class="add-on"><img src="themes/default/images/helios/cal_icon.png" border="0" alt=""></span></td>
                    <td data-i18n="project.create.label.enddate" name="enddate-lbl"></td>
                    <td colspan="2" class="enddate" name="enddateholder"><input name="enddate" value="{{endDate}}" id="endDate" type="text" disabled><span class="add-on"><span class="add-on"><img class="cal_img1" src="themes/default/images/helios/cal_icon.png" border="0" alt=""></span></span></td>
                </tr>
				<tr>
					{{#if editProject.multiModule}}
                	   <td><input type="checkbox" name="multimodule" value="true" checked><b>&nbsp; Multi Module</b></td>
					{{else}}
						<td><input type="checkbox" name="multimodule" value="false"><b>&nbsp; Multi Module</b></td>
					{{/if}}   
                    <td><input type="text" placeholder="Artifact ID"></td>
                    <td colspan="6"></td>
                </tr>
			{{/editProject}}
               <tr id="appLayaer" class="proj_title">
                  <td colspan="4">{{applicationlayerData.name}}</td>
                  <td colspan="4"><img name="close" border="0" alt=""><img name="close" id="applicationlayer" src="themes/default/images/helios/close_icon.png"  border="0" alt=""></td>
                </tr>
                <tr class="applnLayer" key="displayed">
						<td colspan="8" class="appln">
							<table class="table table-striped table_border table-bordered applnlayer" cellpadding="0" cellspacing="0" border="0">
								<thead>
									<tr>
										<th><span data-i18n="project.create.label.appcode"></span><sup>*</sup></th>
										<th><span data-i18n="project.create.label.technology"></span></th>
										<th><span data-i18n="project.create.label.applicationversion"></span></th>
										<th></th>
										<th></th>
										<th colspan="3"><span data-i18n="project.create.label.dependency"></span></th>
									</tr>
								</thead>
								<tbody class="applnlayer"></tbody>
                           </table>
						</td>
					</tr>
					 <tr id="webLayers" class="proj_title">
                      <td colspan="4">{{weblayerData.name}}</td>
                      <td colspan="4"><img name="close" id="weblayer" src="themes/default/images/helios/close_icon.png"   border="0" alt=""></td>
                     </tr>
                     <tr class="webLayer" key="displayed">
						  <td colspan="8" class="WebLayer">
							<table class="table table-striped table_border table-bordered WebLayer" cellpadding="0" cellspacing="0" border="0">
								<thead>
									<tr>
										<th><span data-i18n="project.create.label.appcode"></span><sup>*</sup></th>
										<th><span data-i18n="project.create.label.weblayer"></span></td>
										<th><span data-i18n="project.create.label.widget"></span></td>
										<th><span data-i18n="project.create.label.widgetversion"></span></td>
										<th><span data-i18n="project.create.label.dependency"></span></th>
										<th></th>
									</tr>
								</thead>
								<tbody class="WebLayer"></tbody>
                           </table>
                        </td>	
                      </tr>
					  <tr id="mobLayers" class="proj_title">
                      <td colspan="4">{{mobilelayerData.name}}</td>
                      <td colspan="4"><img name="close" id="mobilelayer" src="themes/default/images/helios/close_icon.png"  border="0" alt=""></td>
                     </tr>
                     <tr class="mobLayer" key="displayed">
                      	<td colspan="8" class="mob">
							<table class="table table-striped table_border table-bordered mob-table" cellpadding="0" cellspacing="0" border="0">
								<thead>
									<tr>
										<th><span data-i18n="project.create.label.appcode"></span><sup>*</sup></th>
										<th><span data-i18n="project.create.label.mobile"></span></th>
										<th><span data-i18n="project.create.label.types"></span></th>
										<th><span data-i18n="project.create.label.mobileversion"></span></th>
										<th></th>
										<th></th>
									</tr>
								</thead>
								<tbody class="MobLayer"></tbody>
                           </table>
                        </td>
                     </tr>
              	</tbody>
            </table>
    	<!--content_main div ends here -->
    </section>
    <section class="content_end">
		<div class="flt_left">
        	<input type="submit" name="applicationlayer" value="Application Layer" class="btn btn_style app_layer_icon_btn" style="display:none;">
            <input type="submit" name="weblayer" value="Web Layer" class="btn btn_style web_layer_icon_btn" style="display:none;">
            <input type="submit" name="mobilelayer" value="Mobile Layer" class="btn btn_style mobile_layer_icon_btn" style="display:none;">
        </div>
        <div class="flt_right">
        	{{#if userPermissions.manageApplication}}
        		<input type="submit" value="Update" id="updateProject" class="btn btn_style add_icon_btn">
        	{{else}}
        		<input type="submit" value="Update" id="updateProject" class="btn btn_style add_icon_btn" disabled>
    		{{/if}}
            <input type="submit" value="Cancel" id="cancelUpdate" class="btn btn_style cancel_icon_btn">
        </div>
    	<!--content_title ends here -->
    </section>
</section>	