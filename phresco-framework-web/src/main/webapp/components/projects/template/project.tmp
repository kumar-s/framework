<section id="addproject">    
	<section class="content_title">
    	<h1 id="projectTitle">Create Project</h1>
    	<!--content_title ends here -->
    </section>
	
    <section class="content_main create_proj">
          <table class="table table-striped table_border table-bordered create_edit_table" cellpadding="0" cellspacing="0" border="0">  
            <tbody name="layercontents"> 
            	<tr>
                    <td data-i18n="project.create.label.name"><sup>*</sup></td>
                    <td><input type="text" name="projectname" placeholder="Project Name"/></td>
                    <td data-i18n="project.create.label.code"><sup>*</sup></td>
                    <td><input type="text" name="projectcode" placeholder="Project Code"/></td>
                    <td data-i18n="project.create.label.version"></td>
                    <td><input type="text" name="projectversion"/></td>
                    <td data-i18n="project.create.label.description"></td>
                    <td><input type="text" name="projectdescription" data-i18n="[placeholder]project.create.label.description"/></td>
                </tr>
                <tr>
                    <td data-i18n="project.create.label.startdate"></td>
                    <td class="startdate"><div class="input-append date" id="startDate" data-date="13-05-2013" data-date-format="dd-mm-yyyy"><input name="startdate" type="text" disabled><span class="add-on"><img src="../themes/default/images/helios/cal_icon.png" border="0" alt=""></span></div></td>
                    <td data-i18n="project.create.label.enddate"></td>
                    <td class="enddate"><div class="input-append date" id="endDate" data-date="14-05-2013" data-date-format="dd-mm-yyyy"><input name="enddate" type="text" disabled><span class="add-on"><img src="../themes/default/images/helios/cal_icon.png"   border="0" alt=""></span></div></td>
                    <td colspan="2">
                        <select class="custon_dropdown">
                            <option>Pre Built</option>
                            <option>Custom</option>
                        </select>
                        <select class="custon_dropdown">
                            <option>Select</option>
                        </select>
                    </td>
                    <td data-i18n="project.create.label.pilotprojects"></td>
                    <td>
                        <select>
                            <option>Select Pilot Projects</option>
                            <option>Select Pilot Projects</option>
                            <option>Select Pilot Projects</option>
                        </select>
                    </td>
                </tr>
				{{#if applicationlayerData.techGroups}}
					<tr class="proj_title">
					  <td colspan="4">{{applicationlayerData.name}}</td>
					  <td colspan="4"><img name="close" id="applicationlayer" src="../themes/default/images/helios/close_icon.png"   border="0" alt=""></td>
					</tr>
					<tr class="applnLayer" name="staticApplnLayer" key="displayed">
					  <td data-i18n="project.create.label.appcode"></td>	
					  <td class="applnappcode"><input type="text" class="appln-appcode"></td>
					  <td data-i18n="project.create.label.technology"></td>
					  <td name="technology" class="technology">
						  <select name="appln_technology" class="appln_technology">
							<option>Select Technology</option>
							{{#each applicationlayerData.techGroups}}
								{{#each techInfos}}
								  <option value="{{id}}">{{name}}</option>
								{{/each}}	
							{{/each}}
						  </select>
					  </td>
					  <td data-i18n="project.create.label.applicationversion"></td>
					  <td colspan="3" name="version" class="version">
						<select name="appln_version" class="appln_version">
							<option>Select Version</option>
						</select>
						<div class="flt_right icon_center">
						  <a href="javascript:;" name="addApplnLayer"><img src="../themes/default/images/helios/plus_icon.png"  border="0" alt=""></a>
						  <a href="javascript:;" name="removeApplnLayer"></a>
						</div>
					  </td>
					</tr>
				 {{/if}}
				 {{#if weblayerData.techGroups}}
                     <tr class="proj_title">
                      <td colspan="4">{{weblayerData.name}}</td>
                      <td colspan="4"><img name="close" id="weblayer" src="../themes/default/images/helios/close_icon.png"   border="0" alt=""></td>
                     </tr>
                     <tr class="webLayer" name="staticWebLayer" key="displayed">
                      <td data-i18n="project.create.label.appcode"></td>	
	                  <td class="webappcode"><input type="text" class="web-appcode"></td>
                      <td data-i18n="project.create.label.weblayer"></td>	
                      <td name="web" class="web">
                      	  <select name="weblayer" class="weblayer">
                            <option>Select Web Layer</option>
							{{#each weblayerData.techGroups}}
								<option value="{{id}}">{{name}}</option>
							{{/each}}
                          </select>
                          <span class="loading_bar" style="display:none;"><img src="../themes/default/images/helios/loading.gif" width="16" height="16" border="0"></span>
                      </td>
                      <td data-i18n="project.create.label.widget"></td>
                      <td name="widget" class="widget">
                      	 <select name="web_widget" class="web_widget">
                            <option>Select Widget</option>
                         </select>
                      </td>
                      <td data-i18n="project.create.label.widgetversion"></td>
                      <td name="widgetversion" class="widgetversion">
                      	<select name="web_version" class="web_version">
                            <option>Select Version</option>
                        </select>
                        <div class="flt_right icon_center">
							<a href="javascript:;" name="addWebLayer"><img  src="../themes/default/images/helios/plus_icon.png"  border="0" alt=""> </a>
							<a href="javascript:;" name="removeWebLayer"></a>
						</div>
                       </td>
                      </tr>
					{{/if}}
				{{#if mobilelayerData.techGroups}}
                    <tr class="proj_title">
                      <td colspan="4">{{mobilelayerData.name}}</td>
                      <td colspan="4"><img name="close" id="mobilelayer" src="../themes/default/images/helios/close_icon.png"  border="0" alt=""></td>
                     </tr>
                     <tr class="mobLayer" key="displayed">
                      <td colspan="8" class="mob">
                          <table width="100%" cellpadding="0" cellspacing="0" border="0" class="mob-table">
							  <tr class="mobileLayer" name="staticMobileLayer">
									<td data-i18n="project.create.label.appcode"></td>	
									<td class="mobileappcode"><input type="text" class="mobile-appcode"></td>
									<td data-i18n="project.create.label.mobile"></td>		
									<td name="mobile" class="mobile">
										<select name="mobile_layer" class="mobile_layer">
											<option>Select Model</option>
											{{#each mobilelayerData.techGroups}}
												<option value="{{id}}">{{name}}</option>
											{{/each}}
										</select>
									</td>
									<td data-i18n="project.create.label.types"></td>
									<td name="types" class="types">       
										<select name="mobile_types" class="mobile_types">
											<option>Select Type</option>
										</select>
									</td>
									<td data-i18n="project.create.label.mobileversion"></td>
									<td name="mobileversion" class="mobileversion">      
										<select name="mobile_version" class="mobile_version">
											<option>Select Version</option>
										</select>
									</td>
									<td style="padding-right:0;">
										  <input type="checkbox"> <font data-i18n="project.create.label.mobile"></font> &nbsp;&nbsp;&nbsp;<input type="checkbox"> <font data-i18n="project.create.label.tablet"></font>&nbsp;&nbsp;&nbsp;
										<div class="flt_right icon_center">
											<a href="javascript:;" name="addMobileLayer"><img src="../themes/default/images/helios/plus_icon.png"  border="0" alt=""></a> 
											<a href="javascript:;" name="removeMobileLayer"></a>
										</div> 
									</td>
							  </tr>
                          </table>
                      </td>	
                    </tr>
					{{/if}}
              	</tbody>
            </table>
    	<!--content_main div ends here -->
    </section>
    <section class="content_end">
    	<div class="flt_left">
        	<input type="submit" name="applicationlayer" value="Application Layer" class="btn btn_style app_layer_icon_btn" disabled>
            <input type="submit" name="weblayer" value="Web Layer" class="btn btn_style web_layer_icon_btn" disabled>
            <input type="submit" name="mobilelayer" value="Mobile Layer" class="btn btn_style mobile_layer_icon_btn" disabled>
        </div>
        <div class="flt_right">
        	<input type="submit" value="Create" name="Create" class="btn btn_style add_icon_btn">
            <input type="submit" value="Cancel" name="Cancel" class="btn btn_style cancel_icon_btn">
        </div>
    	<!--content_title ends here -->
    </section>
</section>	