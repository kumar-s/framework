<section id="addproject">    
    <section class="content_main create_proj">
		<div class="scrolldiv">
          <table class="table table-striped table_border table-bordered create_edit_table" cellpadding="0" cellspacing="0" border="0">  
            <tbody name="layercontents"> 
            	<tr>
                    <td><span data-i18n="project.create.label.name"></span><sup>*</sup></td>
                    <td class="right-align"><input type="text" name="projectname" id="projectname" maxlength="30" data-i18n="[placeholder]project.create.label.projectname" title="30 Characters only"/></td>
                    <td><span data-i18n="project.create.label.code"></span><sup>*</sup></td>
                    <td><input type="text" name="projectcode" id="projectcode" maxlength="30" data-i18n="[placeholder]project.create.label.projectcode" title="30 Characters only"/></td>
                    <td><span data-i18n="project.create.label.version"></span><sup>*</sup></td>
                    <td><input type="text" name="projectversion" id="projectversion" value="1.0" maxlength="20" title="20 Characters only"/></td>
                    <td data-i18n="project.create.label.description"></td>
                    <td rowspan="2" style="vertical-align: top;"><textarea class="popuptextarea" name="projectdescription" id="projectdescription" maxlength="150" title="150 Characters only" data-i18n="[placeholder]project.create.label.description"/></td>
                </tr>
                <tr>
                    <td colspan="2">
                        <select class="custon_dropdown" name="builtmyself">
                            <option value="custom" data-i18n="project.create.label.custom"></option>
                            <option value="prebuilt" data-i18n="project.create.label.prebuilt"></option>
                        </select>
                        <select class="optional_dropdown" name="prebuiltapps">
                            <option data-i18n="project.create.label.selpilotproject"></option>
                        </select>
						<select class="optional_dropdown" name="builtmyselfapps" disabled>
                            <option>Select</option>
                        </select>
                    </td>
					<td data-i18n="project.create.label.startdate" name="startdate-lbl"></td>
                    <td class="startdate" name="startdateholder"><input name="startdate" type="text" id="startDate"><span class="add-on"><img id="strdt" src="themes/default/images/helios/cal_icon.png" border="0" alt=""></span></td>
                    <td data-i18n="project.create.label.enddate" name="enddate-lbl"></td>
                    <td colspan="2" class="enddate" name="enddateholder"><input name="enddate" id="endDate" type="text" disabled><span class="add-on"><span class="add-on"><img id="enddt" src="themes/default/images/helios/cal_icon.png" border="0" alt=""></span></span></td>
                </tr>
				<tr>	
                	<td colspan="2"><input type="checkbox" name="multimodule" value="false"><b data-i18n="project.create.label.multimodule"></b></td>
                    <td><span name="artifactId-lbl" data-i18n="project.create.label.artifactId" style="display:none;"></span></td>
					<td><input name="artifactId" type="text" maxlength="150" data-i18n="[placeholder]project.create.label.artifactId" style="display:none;"></td>
                    <td colspan="6"></td>
                </tr>
                {{#if applicationlayerData.techGroups}}
				<tr class="proj_title" name="applicationlayer">
					<td colspan="3">{{applicationlayerData.name}}</td>
					<td colspan="3"><span class="errmsg1"></span></td>
					<td colspan="2"><img name="close" id="applicationlayer" src="themes/default/images/helios/close_icon.png"  border="0" alt=""></td>
				</tr>
				<tr class="applnLayer" name="appvalidation" key="displayed">
					<td colspan="8" class="appln">
						<table class="table table-striped table_border border_div table-bordered applnlayer" cellpadding="0" cellspacing="0" border="0">
							<thead>
								<tr>
									<th><span data-i18n="project.create.label.module"></span><sup>*</sup></th>
									<th><span data-i18n="project.create.label.technology"></span></th>
									<th><span data-i18n="project.create.label.version"></span></th>
									<th></th>
									<th></th>
									<th colspan="3"><span name="appdependency" data-i18n="project.create.label.dependency" style="display:none;"></span></th>
								</tr>
							</thead>
							<tbody class="applnlayer">
								<tr class="applnlayercontent" name="staticApplnLayer">
									<td class="applnappcode"><input type="text" id="appcode"  maxlength="30" title="30 characters only" class="appln-appcode appCodeText" count=1></td>
									<td name="technology" class="technology">
										<select name="appln_technology" class="appln_technology selectpicker" title="Select Technology">
										   <option value="Select Technology">Select Technology</option>
										   {{#each applicationlayerData.techGroups}}
												{{#each techInfos}}
												  <option value="{{id}}">{{name}}</option>
												{{/each}}	
											{{/each}}
										</select>
									 </td>
									 <td colspan="3" name="version" class="version">
										<select name="appln_version" class="appln_version selectpicker" title="Select Version">
										   <option value="Select Version">Select Version</option>
										</select>
									 </td>
									 <td colspan="2" class="appdependencyTd">
										<select class="selectpicker appdependencySelect" name="appdependencySelect" multiple title="Select Dependency" data-selected-text-format="count>2">
										   <option value="0" disabled data-i18n="project.create.label.seldependency"></option>
										</select>
									 </td>
									 <td>
										<div class="flt_right icon_center"><a name="addApplnLayer"><img src="themes/default/images/helios/plus_icon.png" border="0" alt=""></a><a href="javascript:;" name="removeApplnLayer"></a></div>
									 </td>
							    </tr>
						  </tbody>
                       </table>
					</td>
				</tr>
				{{/if}}
				{{#if weblayerData.techGroups}}
				<tr class="proj_title" name="weblayer">
					<td colspan="3">{{weblayerData.name}}</td>
					<td colspan="3"><span class="errmsg2"></span></td>
					<td colspan="2"><img name="close" id="weblayer" src="themes/default/images/helios/close_icon.png"   border="0" alt=""></td>
				</tr>
				<tr class="webLayer" name="appvalidation" key="displayed">
					<td colspan="8" class="WebLayer">
						<table class="table table-striped table_border border_div table-bordered WebLayer" cellpadding="0" cellspacing="0" border="0">
							<thead>
								<tr>
									<th><span data-i18n="project.create.label.module"></span><sup>*</sup></th>
									<th><span data-i18n="project.create.label.weblayer"></span></th>
									<th><span data-i18n="project.create.label.widget"></span></th>
									<th><span data-i18n="project.create.label.version"></span></th>
									<th><span name="webdependency" data-i18n="project.create.label.dependency" style="display:none;"></span></th>
									<th></th>
								</tr>
							</thead>
							<tbody class="WebLayer">
								<tr class="weblayercontent" name="staticWebLayer">
									<td class="webappcode"><input type="text" id="webappcode"  maxlength="30" title="30 characters only" class="web-appcode appCodeText" count=2></td>
									<td name="web" class="web">
										<select name="weblayer" class="weblayer selectpicker" title="Select Layer">
											<option value="Select Layer">Select Layer</option>
											{{#each weblayerData.techGroups}}
												<option value="{{id}}">{{name}}</option>
											{{/each}}
										</select>
									 </td>
									 <td name="widget" class="widget">
										<select name="web_widget" class="web_widget selectpicker" title="Select Widget">
											<option value="Select Widget">Select Widget</option>
										 </select>
									 </td>
									 <td name="widgetversion" class="widgetversion">
										<select name="web_version" class="web_version selectpicker" title="Select Version">
											<option value="Select Version">Select Version</option>
										</select>
									 </td>
									<td class="webdependencyTd">
											<select class="webdependencySelect selectpicker" name="webdependencySelect"  multiple data-selected-text-format="count>2" title="Select Dependency">
											   <option disabled data-i18n="project.create.label.seldependency"></option>
											</select>
										 </td>
									 <td>
										<div class="flt_right icon_center"><a href="javascript:;" name="addWebLayer"><img src="themes/default/images/helios/plus_icon.png" border="0" alt=""></a><a href="javascript:;" name="removeWebLayer"></a></div>
									 </td>
								</tr>
							</tbody>
						</table>
                    </td>
				</tr>
				{{/if}}
				{{#if mobilelayerData.techGroups}}
                    <tr class="proj_title" name="mobilelayer">
                      <td colspan="3">{{mobilelayerData.name}}</td>
                      <td colspan="3"><span class="errmsg3"></span></td>
                      <td colspan="2"><img name="close" id="mobilelayer" src="themes/default/images/helios/close_icon.png"  border="0" alt=""></td>
                     </tr>
                     <tr class="mobLayer" name="appvalidation" key="displayed">
                      	<td colspan="8" class="mob">
							<table class="table table-striped table_border border_div table-bordered mob-table" cellpadding="0" cellspacing="0" border="0">
								<thead>
									<tr>
										<th><span data-i18n="project.create.label.module"></span><sup>*</sup></th>
										<th><span data-i18n="project.create.label.technology"></span></th>
										<th><span data-i18n="project.create.label.platform"></span></th>
										<th><span data-i18n="project.create.label.version"></span></th>
										<th></th>
										<th></th>
									</tr>
								</thead>
								<tbody>
									<tr class="mobilelayercontent" name="staticMobileLayer">
										<td class="mobileappcode"><input type="text" id="mobileappcode" maxlength="30" title="30 characters only" class="mobile-appcode appCodeText" count=3></td>
										<td name="mobile" class="mobile">
											<select name="mobile_layer" class="mobile_layer selectpicker" title="Select Model">
											   <option value="Select Technology">Select Technology</option>
											   {{#each mobilelayerData.techGroups}}
												  <option value="{{id}}">{{name}}</option>
											   {{/each}}
											</select>
										 </td>
										 <td name="types" class="types">
											<select name="mobile_types" class="mobile_types selectpicker" title="Select Type">
											   <option value="Select Platform">Select Platform</option>
											</select>
										 </td>
										 <td colspan="2" name="mobileversion" class="mobileversion">
											<select name="mobile_version" class="mobile_version selectpicker" title="Select Version">
											   <option value="Select Version">Select Version</option>
											</select>
										 </td>
										 <td>
											<div class="flt_right icon_center"><a href="javascript:;" name="addMobileLayer"><img src="themes/default/images/helios/plus_icon.png" border="0" alt=""></a><a href="javascript:;" name="removeMobileLayer"></a> </div>
										 </td>
									</tr>
							   </tbody>
                           </table>
                        </td>
                     </tr>
					{{/if}}
              	</tbody>
            </table>
		</div>
    	<!--content_main div ends here -->
    </section>
    <section class="content_end">
    	<div class="flt_left">
        	<input type="submit" name="applicationlayer" value="Application Layer" data-i18n="[value]project.create.title.label.applicationlayer" class="btn btn_style app_layer_icon_btn" style="display:none;">
            <input type="submit" name="weblayer" value="Web Layer" data-i18n="[value]project.create.title.label.weblayer" class="btn btn_style web_layer_icon_btn" style="display:none;">
            <input type="submit" name="mobilelayer" value="Mobile Layer" data-i18n="[value]project.create.title.label.mobilelayer" class="btn btn_style mobile_layer_icon_btn" style="display:none;">
        </div>
        <div class="flt_right">
        	<input type="submit" value="Create" data-i18n="[value]common.btn.create" name="Create" class="btn btn_style add_icon_btn">
            <input type="submit" value="Cancel" data-i18n="[value]common.btn.close" name="Cancel" class="btn btn_style cancel_icon_btn">
        </div>
		<div class="msgdisplay"></div>
    	<!--content_title ends here -->
    </section>
</section>	