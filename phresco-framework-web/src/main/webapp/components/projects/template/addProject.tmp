<section id="addproject">    
    <section class="content_main create_proj">
          <table class="table table-striped table_border table-bordered create_edit_table" cellpadding="0" cellspacing="0" border="0">  
            <tbody class="scrollContent" name="layercontents"> 
            	<tr>
                    <td><span data-i18n="project.create.label.name"></span><sup>*</sup></td>
                    <td><input type="text" name="projectname" placeholder="Project Name"/></td>
                    <td><span data-i18n="project.create.label.code"></span><sup>*</sup></td>
                    <td><input type="text" name="projectcode" placeholder="Project Code"/></td>
                    <td><span data-i18n="project.create.label.version"></span><sup>*</sup></td>
                    <td><input type="text" name="projectversion" value="1.0"/></td>
                    <td data-i18n="project.create.label.description"></td>
                    <td rowspan="2"><textarea class="popuptextarea" name="projectdescription" data-i18n="[placeholder]project.create.label.description"/></td>
                </tr>
                <tr>
                    <td colspan="2">
                        <select class="custon_dropdown" name="builtmyself">
                            <option>Custom</option>
                            <option>Pre Built</option>
                        </select>
                        <select class="optional_dropdown" name="prebuiltapps">
                            <option>NodeJSandMySQL</option>
                        </select>
						<select class="optional_dropdown" name="builtmyselfapps" disabled>
                            <option>Select</option>
                        </select>
                    </td>
					<td data-i18n="project.create.label.startdate" name="startdate-lbl"></td>
                    <td class="startdate" name="startdateholder"><input name="startdate" type="text" id="startDate"><span class="add-on"><img id="strdt" src="themes/default/images/helios/cal_icon.png" border="0" alt=""></span></td>
                    <td data-i18n="project.create.label.enddate" name="enddate-lbl"></td>
                    <td colspan="2" class="enddate" name="enddateholder"><input name="enddate" id="endDate" type="text"><span class="add-on"><span class="add-on"><img id="enddt" src="themes/default/images/helios/cal_icon.png" border="0" alt=""></span></span></td>
                </tr>
				<tr>	
                	<td colspan="2"><input type="checkbox"><b>&nbsp; Multi Module</b></td>
                    <td colspan="2"><input type="text" placeholder="Artifact ID"></td>
                    <td colspan="6"></td>
                </tr>
				{{#if applicationlayerData.techGroups}}
					<tr class="proj_title">

					 <td colspan="3">{{applicationlayerData.name}}</td>
					 <td colspan="3"><span class="errmsg1"></span></td>
					 <td colspan="2"><img name="close" id="applicationlayer" src="themes/default/images/helios/close_icon.png"  border="0" alt=""></td>
					</tr>
					<tr class="applnLayer" key="displayed">
						<td colspan="8" class="appln">
							<table class="table table-striped table_border table-bordered applnlayer" cellpadding="0" cellspacing="0" border="0">
								<thead>
									<tr>
										<th><span data-i18n="project.create.label.appcode"></span><sup>*</sup></th>
										<th><span data-i18n="project.create.label.technology"></span></th>
										<th><span data-i18n="project.create.label.applicationversion"></span></th>
										<th></th>
										<th></th>
										<th colspan="3"><span data-i18n="project.create.label.dependency"></span></th>
									</tr>
								</thead>
								<tbody class="applnlayer">
									<tr class="applnlayercontent" name="staticApplnLayer">
										<td class="applnappcode"><input type="text" id="appcode" class="appln-appcode"></td>
										<td name="technology" class="technology">
											<select name="appln_technology" class="appln_technology">
											   <option>Select Technology</option>
											   {{#each applicationlayerData.techGroups}}
													{{#each techInfos}}
													  <option value="{{id}}">{{name}}</option>
													{{/each}}	
												{{/each}}
											</select>
										 </td>
										 <td colspan="3" name="version" class="version">
											<select name="appln_version" class="appln_version">
											   <option>Select Version</option>
											</select>
										 </td>
										 <td colspan="2">
											<select>
											   <option>Select Dependency</option>
											</select>
										 </td>
										 <td>
											<div class="flt_right icon_center"><a name="addApplnLayer"><img src="themes/default/images/helios/plus_icon.png" border="0" alt=""></a><a href="javascript:;" name="removeApplnLayer"></a></div>
										 </td>
								    </tr>
							  </tbody>
                           </table>
						</td>
					</tr>
				 {{/if}}
				 {{#if weblayerData.techGroups}}
                     <tr class="proj_title">
                      <td colspan="3">{{weblayerData.name}}</td>
                      <td colspan="3"><span class="errmsg2"></span></td>
                      <td colspan="2"><img name="close" id="weblayer" src="themes/default/images/helios/close_icon.png"   border="0" alt=""></td>
                     </tr>
                     <tr class="webLayer" key="displayed">
						  <td colspan="8" class="WebLayer">
							<table class="table table-striped table_border table-bordered WebLayer" cellpadding="0" cellspacing="0" border="0">
								<thead>
									<tr>
										<th><span data-i18n="project.create.label.appcode"></span><sup>*</sup></th>
										<th><span data-i18n="project.create.label.weblayer"></span></td>
										<th><span data-i18n="project.create.label.widget"></span></td>
										<th><span data-i18n="project.create.label.widgetversion"></span></td>
										<th><span data-i18n="project.create.label.dependency"></span></th>
										<th></th>
									</tr>
								</thead>
								<tbody class="WebLayer">
									<tr class="weblayercontent" name="staticWebLayer">
										<td class="webappcode"><input type="text" id="webappcode" class="web-appcode"></td>
										<td name="web" class="web">
											<select name="weblayer" class="weblayer">
												<option>Select Layer</option>
												{{#each weblayerData.techGroups}}
													<option value="{{id}}">{{name}}</option>
												{{/each}}
											</select>
										 </td>
										 <td name="widget" class="widget">
											<select name="web_widget" class="web_widget">
												<option>Select Widget</option>
											 </select>
										 </td>
										 <td name="widgetversion" class="widgetversion">
											<select name="web_version" class="web_version">
												<option>Select Version</option>
											</select>
										 </td>
										 <td>
											<select>
											   <option>Select Dependency</option>
											</select>
										 </td>
										 <td>
											<div class="flt_right icon_center"><a href="javascript:;" name="addWebLayer"><img src="themes/default/images/helios/plus_icon.png" border="0" alt=""></a><a href="javascript:;" name="removeWebLayer"></a></div>
										 </td>
									</tr>
								</tbody>
                           </table>
                        </td>	
                      </tr>
					{{/if}}
				{{#if mobilelayerData.techGroups}}
                    <tr class="proj_title">
                      <td colspan="3">{{mobilelayerData.name}}</td>
                      <td colspan="3"><span class="errmsg3"></span></td>
                      <td colspan="2"><img name="close" id="mobilelayer" src="themes/default/images/helios/close_icon.png"  border="0" alt=""></td>
                     </tr>
                     <tr class="mobLayer" key="displayed">
                      	<td colspan="8" class="mob">
							<table class="table table-striped table_border table-bordered mob-table" cellpadding="0" cellspacing="0" border="0">
								<thead>
									<tr>
										<th><span data-i18n="project.create.label.appcode"></span><sup>*</sup></th>
										<th><span data-i18n="project.create.label.mobile"></span></th>
										<th><span data-i18n="project.create.label.types"></span></th>
										<th><span data-i18n="project.create.label.mobileversion"></span></th>
										<th></th>
										<th></th>
									</tr>
								</thead>
								<tbody>
									<tr class="mobilelayercontent" name="staticMobileLayer">
										<td class="mobileappcode"><input type="text" id="mobileappcode" class="mobile-appcode"></td>
										<td name="mobile" class="mobile">
											<select name="mobile_layer" class="mobile_layer">
											   <option>Select Model</option>
											   {{#each mobilelayerData.techGroups}}
												  <option value="{{id}}">{{name}}</option>
											   {{/each}}
											</select>
										 </td>
										 <td name="types" class="types">
											<select name="mobile_types" class="mobile_types">
											   <option>Select Type</option>
											</select>
										 </td>
										 <td colspan="2" name="mobileversion" class="mobileversion">
											<select name="mobile_version" class="mobile_version">
											   <option>Select Version</option>
											</select>
										 </td>
										 <td>
											<div class="flt_right icon_center"><a href="javascript:;" name="addMobileLayer"><img src="themes/default/images/helios/plus_icon.png" border="0" alt=""></a><a href="javascript:;" name="removeMobileLayer"></a> </div>
										 </td>
									</tr>
							   </tbody>
                           </table>
                        </td>
                     </tr>
					{{/if}}
              	</tbody>
            </table>
    	<!--content_main div ends here -->
    </section>
    <section class="content_end">
    	<div class="flt_left">
        	<input type="submit" name="applicationlayer" value="Application Layer" class="btn btn_style app_layer_icon_btn" style="display:none;">
            <input type="submit" name="weblayer" value="Web Layer" class="btn btn_style web_layer_icon_btn" style="display:none;">
            <input type="submit" name="mobilelayer" value="Mobile Layer" class="btn btn_style mobile_layer_icon_btn" style="display:none;">
        </div>
        <div class="flt_right">
        	<input type="submit" value="Create" name="Create" class="btn btn_style add_icon_btn">
            <input type="submit" value="Cancel" name="Cancel" class="btn btn_style cancel_icon_btn">
        </div>
    	<!--content_title ends here -->
    </section>
</section>	