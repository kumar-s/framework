<section class="optiontitle qual_unit">
	<div class="unit_text">
    	Functional Test
	</div>
    <div class="flt_right">
		<div class="unit_view hideContent">
			<div class="code_report_icon" id="pdfDiv">
				<div class="icon_images"><a class="tooltiptop" title="" data-placement="bottom" data-toggle="tooltip" href="#" data-original-title="PDF Report"><img src="themes/default/images/helios/pdf_icon_white.png"></a></div>
				<!--Pdf report popup starts here -->
				<div id="pdf_report" class="dyn_popup hideContent">
					<div id="noPdfReport" class="noReport"></div>
                	<table class="table_border availablePdfRptsTbl hideContent" border="0" cellspacing="0" cellpadding="5" width="100%" id="availablePdfRptsTbl">
                		<thead>
                        	<tr>
	                            <th>Existing Reports</th>
	                            <th>Type</th>
	                            <th>Download</th>
	                            <th>Delete</th>
                        	</tr>    
                    	</thead>
                    	<tbody id="availablePdfRptsTbdy">
                    	    
                   	 	</tbody>    
                	</table>
                	<form id="pdfReportForm">
	                   	<table class="report_type">
	                    	<tbody>
	                        	<tr>
	                            	<td>Report Type</td>
	                            	<td>
	                                	<select name="reportDataType">
	                                    	<option value="detail">Detailed Report</option>
	                                    	<option value="crisp">Overall Report</option>
	                                	</select>
	                            	</td>
	                        	</tr>
								<tr>
	                            	<td>Pdf Report Name</td>
		                            <td>
		                               <input type="text" placeholder="Pdf report name" name="pdfName">
		                            </td>
	                        	</tr>	                        	
	                    	</tbody>
	                   	</table>
	                   	<div class="popuploading" id="pdfReportLoading">&nbsp</div>
		               	<div class="flt_right">
		                 	<input type="button" value="Generate" class="btn btn_style" id="generatePdf">
		                 	<input type="button" value="Close" class="btn btn_style dyn_popup_close">
		               	</div>
               			<input type="hidden" name="fromPage" value="functional"/>
           			</form>
				</div>
				<!--Pdf report popup ends here -->
			</div>
            <span>
            	<a id="tabularView" href="#"><img src="themes/default/images/helios/quality_table_on.png" width="25" height="25" border="0" alt=""><b>Table View</b></a>
            </span>
            <span>
            	<a id="graphicalView" href="#"><img src="themes/default/images/helios/quality_graph_off.png" width="26" height="24" border="0" alt="">Graph View</a>
            </span>    
        </div>
    	<div class="icon_images"><a class="tooltiptop" title="" data-placement="bottom" data-toggle="tooltip" href="#" data-original-title="Open Folder"><img src="themes/default/images/helios/openfolder_icon.png" ></a></div>
		<div class="icon_images" style="margin-right:2px;"><a class="tooltiptop" title="" data-placement="bottom" data-toggle="tooltip" href="#" data-original-title="Copy Path"><img src="themes/default/images/helios/copypath_icon.png" ></a></div>
            
        <div class="code_anal">
        	<input type="button" {{#hubButton hubStatus}}{{/hubButton}} class="btn btn_style {{#showHide functionalFramework}}{{/showHide}}">
        	<div id="startHub_popup" class="dyn_popup {{#showHide functionalFramework}}{{/showHide}} dynamic_cont" style="display: none; width:310px">
				<form id="startHubForm" style="margin: 0 0 0 0;">
					<ul class="row dynamicControls" id="startHubDynCtrls">
					</ul>
					<div class="dynamicPopupLoading hideContent">&nbsp</div>
					<div class="hiddenControls"></div>
				</form>
                <div class="flt_right">
                    <input type="button" value="Start" class="btn btn_style" id="executeStartHub">
                    <input type="button" value="Close" class="btn btn_style dyn_popup_close">
				</div>
			</div>
			
        	<input type="button" {{#nodeButton nodeStatus}}{{/nodeButton}} class="btn btn_style {{#showHide functionalFramework}}{{/showHide}}">
        	<div id="startNode_popup" class="dyn_popup {{#showHide functionalFramework}}{{/showHide}} dynamic_cont" style="display: none;  width:310px">
				<form id="startNodeForm" style="margin: 0 0 0 0;">
					<ul class="row dynamicControls" id="startNodeDynCtrls">
					</ul>
					<div class="dynamicPopupLoading hideContent">&nbsp</div>
					<div class="hiddenControls"></div>
				</form>
                <div class="flt_right">
                    <input type="button" value="Start" class="btn btn_style" id="executeStartNode">
                    <input type="button" value="Close" class="btn btn_style dyn_popup_close">
				</div>
			</div>
			
			{{#if userPermissions.manageTests}}
            	<input type="button" value="Test" id="functionalTestBtn" {{#enableDisable functionalFramework hubStatus}}{{/enableDisable}} class="btn btn_style">
        	{{else}}
        		<input type="button" value="Test" class="btn btn_style" disabled>
        	{{/if}}        	
        		                
			<div id="functionalTest_popup" class="dyn_popup hideContent dynamic_cont" style="width: 310px;">
				<form id="functionalTestForm" style="margin: 0 0 0 0;">
					<ul class="row dynamicControls" id="functionalTestDynCtrls">
					</ul>
					<div class="dynamicPopupLoading hideContent">&nbsp</div>
					<div class="hiddenControls"></div>
				</form>
                <div class="flt_right">
                    <input type="button" value="Test" class="btn btn_style" id="runFunctionalTest">
                    <input type="button" value="Close" class="btn btn_style dyn_popup_close">
				</div>
			</div>
		</div>
	</div>
</section>

<div class="mainContent" id="testResult"></div>