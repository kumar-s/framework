<section class="content_main">
		<table class="table table-striped table_border table-bordered" id="projectlist" cellpadding="0" cellspacing="0" border="0">
              <thead>
                 <tr>
                  <th data-i18n="projectlist.label.name"></th>
                  <th data-i18n="projectlist.label.description">Description</th>
                  <th data-i18n="projectlist.label.technology">Technology</th>
                  <th data-i18n="projectlist.label.report">Report</th>
                  <th data-i18n="projectlist.label.repository">Repository</th>
                  <th>Delete ( All <a class="tooltiptop" title="" data-placement="top" data-toggle="tooltip" href="#" data-original-title="Delete All"><img src="../themes/default/images/helios/delete_row.png" width="14" height="18" border="0" alt=""></a> )</th>                  
                  <th>&nbsp;</th>
                </tr>
              </thead>
              <tbody>
                  {{#each row}}
                      <tr class="proj_title">
                        <td colspan="4">{{name}}</td> 
                        <td colspan="3" align="right"><a class="tooltiptop" title="" data-placement="top" data-toggle="tooltip"  data-original-title="Edit Project"><img id="editproject" src="../themes/default/images/helios/edit_row_icon.png" width="29" height="23" border="0" alt=""></a> <a class="tooltiptop" title="" data-placement="top" data-toggle="tooltip" href="#" data-original-title="Delete Project"><img src="../themes/default/images/helios/delete_row_icon.png" width="29" height="23" border="0" alt="0"></a></td>
                      </tr>
                      {{#each appInfos}}
                      <tr>
                        <td name="editApplication" href="#">{{name}}</td>
                        <td>{{description}}</td>
                        <td>{{techInfo.id}}</td>
                        <td><a class="tooltiptop" title="" data-placement="top" data-toggle="tooltip" href="#" data-original-title="PDF Report"><img src="../themes/default/images/helios/pdf_icon.png" width="14" height="16" border="0" alt=""></a></td>
                        <td><a class="tooltiptop" title="" data-placement="top" data-toggle="tooltip" href="#" data-original-title="Add Repo"><img src="../themes/default/images/helios/add_repo_icon.png" width="18" height="15" border="0" alt=""></a> <a class="tooltiptop" title="" data-placement="top" data-toggle="tooltip" href="#" data-original-title="Commit"><img src="../themes/default/images/helios/commit_icon.png" width="16" height="15" border="0" alt=""></a> <a class="tooltiptop" title="" data-placement="top" data-toggle="tooltip" href="#" data-original-title="SVN Update"><img src="../themes/default/images/helios/update_svn_icon.png" width="22" height="15" border="0" alt=""></a></td>
                         <td><a class="tooltiptop" title="" data-placement="top" data-toggle="tooltip" href="#" data-original-title="Delete Row"><img src="../themes/default/images/helios/delete_row.png" width="14" height="18" border="0" alt=""></a></td>
                        <td>&nbsp;</td>
                      </tr>
                    {{/each}}
                  {{/each}}
              </tbody>


            </table>