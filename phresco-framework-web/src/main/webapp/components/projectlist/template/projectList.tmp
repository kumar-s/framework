<section class="content_main proj_list">
	{{#if projectlist}}
		<div class="fixed-table-container">
        <div class="header-background"> </div>
        <div class="fixed-table-container-inner"> 
		<table class="table table-striped table_border table-hover table-bordered" id="projectlist" cellpadding="0" cellspacing="0" border="0">
              <thead class="height_th">
                 <tr>
                  <th><div class="th-inner" data-i18n="projectlist.label.name"></div></th>
                  <th><div class="th-inner" data-i18n="projectlist.label.description"></div></th>
                  <th><div class="th-inner" data-i18n="projectlist.label.technology"></div></th>
                  <th><div class="th-inner" data-i18n="projectlist.label.report"></div></th>
                  <th><div class="th-inner" data-i18n="projectlist.label.repository"></div></th>
                  <th><div class="th-inner" data-i18n="commonlabel.label.delete"></div></th>                  
                  <th><div class="th-inner">&nbsp;</div></th>
                </tr>
              </thead>
              <tbody>
                  {{#each projectlist}}
                      <tr class="proj_title"> 
                        <td colspan="4">{{name}}</td>  
                        <td colspan="3" align="right" class="delimages">
                        	<a href="javascript:void(0)" class="tooltiptop" title="" data-placement="bottom" data-toggle="tooltip"  data-original-title="Edit Project">
                        		<img name="editproject" key="{{id}}" projectcode="{{projectCode}}" projectId="{{id}}" src="themes/default/images/Phresco/edit_row_icon.png" width="29" height="23" border="0" alt="">
                    		</a> 
                    		{{#if ../userPermissions.manageApplication}}
	                    		<a name="{{id}}" appIds="{{#csvAppIds appInfos}}{{/csvAppIds}}" class="tooltiptop del_project" title="" data-placement="bottom" data-toggle="tooltip" href="#" data-original-title="Delete {{name}}">
	                    			<img src="themes/default/images/Phresco/delete_row_icon.png" width="29" height="23" border="0" alt="0">
	                			</a>
                			{{else}}
                				<img src="themes/default/images/Phresco/delete_row_icon_off.png" width="29" height="23" border="0" alt="0">
                			{{/if}}

                            <div id="{{id}}" class="dyn_popup deleteproj_msg"> Are you sure to delete this ?
                                <div>
                                    <input type="button" value="Yes" data-i18n="[value]common.btn.yes" class="btn btn_style" name="holeDelete" deleteAppname="{{name}}">
                                    <input type="button" value="No" data-i18n="[value]common.btn.no" class="btn btn_style dyn_popup_close">
                                </div>
                            </div>
                        </td>
                      </tr>
					         {{#each appInfos}}
                      <tr class="{{appDirName}}" techid="{{techInfo.id}}">
                        <td width="25%"><a name="editApplication" appid="{{id}}" href="javascript:;">{{name}}</a></td>
                        <td width="25%">{{#descriptionshow description}}{{/descriptionshow}}</td>
                        <td width="25%">{{techInfo.name}}</td>
                       <td class="list_img"><a name="pdf_report_{{id}}" temp="pdf_report" fromPage="All" class="tooltiptop" title="" dynamicId="{{id}}" data-placement="bottom" data-t="tooltip" href="javascript:void(0)" data-original-title="PDF Report"><img src="themes/default/images/Phresco/pdf_icon.png" width="14" height="16" border="0" alt=""></a>
                  
              <div id="pdf_report_{{id}}" class="dyn_popup pdf_report">
              <div id="noReport_{{id}}" class="alert noReport hideContent" style="text-align: center;"></div>
				  <table class="hideContent table" name="pdfHeader_{{id}}" show="true">
						  <thead>  
							<tr>
								<th class="" data-i18n="commonlabel.label.existingreport"></th>
								<th class="" data-i18n="commonlabel.label.type"></th>
								<th class="" data-i18n="commonlabel.label.download"></th>
								<th class="" data-i18n="commonlabel.label.delete"></th>
							</tr>
						  </thead>			
				  </table>	
				  <div class = "pdfheight">
				  <table class="existing_report" width="100%" border="0" cellspacing="0" cellpadding="5">
						<tbody name="generatedPdfs_{{id}}" class="scroll">
						</tbody>    
				  </table>
				  </div>	  
                <form id="pdfReport">
                   <table class="report_type">
                    <tbody>
                        <tr>
                        	<td data-i18n="commonlabel.label.reporttype"></td>
                        	<td>
                            	<select name="reportDataType" class="reportDataType">
                                	<option value="detail" data-i18n="projectlist.label.detailedReport"></option>
                                	<option value="crisp" data-i18n="projectlist.label.overallReport"></option>
                            	</select>
                        	</td>
                    	</tr>
                         <tr>
                        	<td data-i18n="commonlabel.label.pdfreportname"></td>
                            <td>
                               <input type="text" data-i18n="[placeholder]projectlist.label.pdfreportname" name="pdfName" class="pdfName">
                            </td>
                        </tr>
                    </tbody>
                   </table>
                   <div class="popuploading" id="pdfReportLoading_{{id}}">&nbsp</div>
                <div class="flt_right">
                {{#if ../../userPermissions.managePdfReports}}
                 	<input type="button" fromPage="All"  appDirName="{{appDirName}}" value="Generate" data-i18n="[value]projectlist.label.generate" class="btn btn_style" name="generate" appId="{{id}}" projectId = "{{../id}}">
                 {{else}}
                 	<input type="button" disabled=true fromPage="All"  appDirName="{{appDirName}}" value="Generate" data-i18n="[value]projectlist.label.generate" class="btn btn_style" name="generate" appId="{{id}}" projectId = "{{../id}}">
                  {{/if}}
                 <input type="button" value="Close" data-i18n="[value]common.btn.close" class="btn btn_style dyn_popup_close">
               </div>
               <input type="hidden" name="fromPage" value="All" data-i18n="[value]common.btn.all"/>
               </form>
                <!--pdf_report div ends here -->  
             </div> 
                  </td>
                        <td class="list_img">
                        {{#if ../../userPermissions.manageRepo}}
							<a name="addRepo_{{id}}" dynamicId="{{id}}" class="tooltiptop" title="" data-placement="bottom" data-toggle="tooltip" href="javascript:void(0)" data-original-title="Add Repo" >
								<img src="themes/default/images/Phresco/add_repo_icon.png" width="18" height="15" border="0" alt="" class="left_add_repo">
							</a> 

                        	<a name="commit{{id}}" dynamicId="{{id}}" class="tooltiptop" title="" data-placement="bottom" data-toggle="tooltip" href="javascript:void(0)" data-original-title="Commit" >
                        		<img src="themes/default/images/Phresco/commit_icon.png" width="16" height="15" border="0" alt="" class="centre_img">
                    		</a> 
						{{else}}
                        	<img src="themes/default/images/Phresco/add_repo_icon_off.png" width="18" height="15" border="0" alt="" class="left_add_repo">
                        	<img src="themes/default/images/Phresco/commit_icon_off.png" width="16" height="15" border="0" alt="">
                        {{/if}}
                        
                        {{#if ../../userPermissions.manageRepo}}
                        	<a name="svn_update{{id}}" dynamicId="{{id}}" class="tooltiptop" title="" data-placement="bottom" data-toggle="tooltip" href="javascript:void(0)" data-original-title="Update" >
                        		<img src="themes/default/images/Phresco/update_svn_icon.png" width="22" height="15" border="0" alt="">
                    		</a>
              			{{else}}
              				<img src="themes/default/images/Phresco/update_svn_icon_off.png" width="22" height="15" border="0" alt="">
              			{{/if}}
                  
                  <div id="addRepo_{{id}}" class="add_repo dyn_popup" name="addRepo">
                      <table width="100%" border="0" cellspacing="0" cellpadding="0">
                        <tbody>
                            <tr>
                                <td><span data-i18n="commonlabel.label.type"></span><sup>*</sup></td>
                                <td><span data-i18n="navigation.application.apprepourl"></span><sup>*</sup></td>
                            </tr>
                            <tr>
                                <td>
                                    <select id="type_{{id}}" class="ad_Select">
                                        <option value="svn" data-i18n="projectlist.label.svn"></option>
                                        <option value="bitkeeper" data-i18n="projectlist.label.bitkeeper"></option>
                                        <option value="git" data-i18n="projectlist.label.git"></option>
                                    </select>
                                </td>
                                <td><input type="text" name="repoUrl" placeholder="Repo Url" id="repourl_{{id}}"></td>
                            </tr>
                            <tr>
                                <td class="seperatetd"><span data-i18n="projectlist.label.othercredential"></span><input type="checkbox" id="repocredential_{{id}}" repoDynid="{{id}}" class="credential"></td>
                                <td></td>
                            </tr>
                            <tr>
                                <td><span data-i18n="login.placeholder.username"></span><sup>*</sup></td>
                                <td><span data-i18n="login.placeholder.password"></span><sup>*</sup></td>
                            </tr>
                            <tr>
                                <td><input type="text" name="username" data-i18n="[placeholder]login.placeholder.username" id="uname_{{id}}" class="uname"></td>
                                <td><input type="password" name="password" placeholder="Password" id="pwd_{{id}}" class="pwd"></td>
                            </tr>
                            <tr>
                                <td colspan="2">
                                    <textarea class="popuptextarea" name="commitMsg" data-i18n="[placeholder]projectlist.label.message" id="repomessage_{{id}}"></textarea><img class="search" src="themes/default/images/Phresco/search_icon.png" border="0" alt="">
								</td>
                            </tr>
							<tr>
								<td><select class="searchdropdown"></select></td>
							</tr>
                        </tbody>
                     </table> 
                     <div class="popuploading" id="addRepoLoading_{{id}}">&nbsp</div>
                     <div class="flt_right" appId="{{id}}" projectId = "{{../id}}" appDirName="{{appDirName}}">
                        <input type="button" name="addrepobtn" id="{{id}}" value="Add To Repo" data-i18n="[value]projectlist.btn.addrepo;" class="btn btn_style">
                        <input type="button" value="Close" data-i18n="[value]common.btn.close;" class="btn btn_style dyn_popup_close">
                     </div> 
                  </div>
                  <!--add_repo div ends here --> 
                  
                  <div id="commit{{id}}" class="commit dyn_popup popup_bg commit_table" name="commitpopup">
                      <div class="hideContent commit_data_{{id}}">
                            <table  class="commitable_files_{{id}}" width="100%" border="0" cellspacing="0" cellpadding="5">
                                  
                            </table>                               
                            <table class="commitData_{{id}} commit_tab" width="100%" border="0" cellspacing="0" cellpadding="0">
                                <tbody>
                                    <tr>
                                        <td><span data-i18n="commonlabel.label.type"></span><sup>*</sup></td>
										<td><span data-i18n="navigation.application.apprepourl"></span><sup>*</sup></td>
                                    </tr>
                                    <tr>
                                        <td>
                                            <select id="commitType_{{id}}" class="co_Select">
                                                <option value="svn" data-i18n="projectlist.label.svn"></option>
                                        		<option value="bitkeeper" data-i18n="projectlist.label.bitkeeper"></option>
                                        		<option value="git" data-i18n="projectlist.label.git"></option>
                                            </select>
                                        </td>
                                        <td><input type="text" name="repoUrl" placeholder="Repo Url" id="commitRepourl_{{id}}"></td>
                                    </tr>
                                    <tr>
                                        <td class="seperatetd"><span data-i18n="projectlist.label.othercredential"></span><input type="checkbox" id="commitCredential_{{id}}" class="credential"></td>
                                        <td></td>
                                    </tr>
                                    <tr>
                                        <td><span data-i18n="login.placeholder.username"></span><sup>*</sup></td>
										<td><span data-i18n="login.placeholder.password"></span><sup>*</sup></td>
                                    </tr>
                                    <tr>
                                        <td><input type="text" data-i18n="[placeholder]login.placeholder.username" id="commitUsername_{{id}}" class="uname" name="username"></td>
                                        <td><input type="password" placeholder="Password" id="commitPassword_{{id}}" class="pwd" name="password"></td>
                                    </tr>
                                    <tr>
                                        <td colspan="2">
                                            <textarea class="popuptextarea" data-i18n="[placeholder]projectlist.label.message" id="commitMessage_{{id}}" name="commitMsg"></textarea>
                                        </td>
                                    </tr>
                                </tbody>
                             </table> 
							 
                         <div class="flt_right roll_data" appDirName="{{appDirName}}">
                          <input type="button" value="Commit" data-i18n="[value]projectlist.label.commit" disabled class="btn" id="{{id}}" name="commitbtn">
                            <input type="button" value="Close" data-i18n="[value]common.btn.close" class="btn btn_style dyn_popup_close">
                         </div>
					 </div> 
				 	<div class="dummyCommit" id="dummyCommit_{{id}}"></div>
				 	<div class="popuploading" id="commitLoading_{{id}}">&nbsp</div>
					<div class="hideContent commitErr_{{id}} popup_btn" width="100%" border="0" cellspacing="0" cellpadding="0">
						<p><span data-i18n="projectlist.label.notworkingcopy"></span></p>
						<div class="popupCloseBtn">
							<input type="button" value="Close" data-i18n="[value]common.btn.close" class="btn btn_style dyn_popup_close">
						</div>		
					 </div>
                  </div>
                   <!--commit div ends here -->	
                   
                  <div id="svn_update{{id}}" class="svn_update dyn_popup popup_bg" name="updatepopup">
				   <div class="hideContent update_data_{{id}}">
					<table width="100%" border="0" cellspacing="0" cellpadding="0">
                        <tbody>
                                    <tr>
                                        <td><span data-i18n="commonlabel.label.type"></span><sup>*</sup></td>
										<td><span data-i18n="navigation.application.apprepourl"></span><sup>*</sup></td>
                                    </tr>
                                    <tr>
                                        <td>
                                            <select id="updateType_{{id}}" class="up_Select">
                                                <option value="svn" data-i18n="projectlist.label.svn"></option>
                                        		<option value="bitkeeper" data-i18n="projectlist.label.bitkeeper"></option>
                                        		<option value="git" data-i18n="projectlist.label.git"></option>
                                            <option value="perforce" data-i18n="projectlist.label.perforce"></option>
                                            </select>
                                        </td>
                                        <td><input type="text" placeholder="Repo Url" id="updateRepourl_{{id}}" name="repoUrl"></td>
                                    </tr>
                                    <tr>
                                        <td class="seperatetd"><span data-i18n="projectlist.label.othercredential"></span><input type="checkbox" id="updateCredential_{{id}}" class="credential"></td>
                                        <td></td>
                                    </tr>
                                    <tr>
                                        <td><span data-i18n="login.placeholder.username"></span><sup>*</sup></td>
										<td><span data-i18n="login.placeholder.password"></span><sup>*</sup></td>
                                    </tr>
                                    <tr>
                                        <td><input type="text" data-i18n="[placeholder]login.placeholder.username" id="updateUsername_{{id}}" class="uname" name="username"></td>
                                        <td><input type="password" placeholder="Password" id="updatePassword_{{id}}" class="pwd" name="password"></td>
                                    </tr>
                                    <tr>
                                       <td colspan="2" data-i18n="projectlist.label.revision"></td>
                                    </tr>
                                    <tr>
                                        <td><input type="radio" name="revision" class="radioBtnClass" value="head" checked><span data-i18n="projectlist.label.headrevision"></span><input type="radio" name="revision" class="radioBtnClass" value=""><span data-i18n="projectlist.label.revision"></span></td>
                                        <td><input class="revision" type="text"></td>
                                    </tr>
                                </tbody>
                     </table> 
                       <div class="perforcedata">
                        <table>
                            <tbody>
                                <tr>
                                    <td><label name="stream" value="stream">Stream</label></td>
                                </tr>
                                <tr>    
                                    <td><input id="stream_{{id}}" name="stream" type="text" class="stream"></td>
                                </tr>
                            </tbody>
                         </table>
                     </div>
                     <div class="flt_right" style=margin-top:5px;" appDirName="{{appDirName}}">
                         <input type="button" value="Update Application" data-i18n="[value]common.btn.updateapp"  id="{{id}}" class="btn btn_style" name="updatebtn">
                         <input type="button" value="Close" data-i18n="[value]common.btn.close" class="btn btn_style dyn_popup_close">
                     </div>    
					</div>
					<div class="dummyUpdate" id="dummyUpdate_{{id}}"></div>
				 	<div class="popuploading" id="updateLoading_{{id}}">&nbsp</div>
					<div class="hideContent updateErr_{{id}} popup_btn" width="100%" border="0" cellspacing="0" cellpadding="0">
						<p><span data-i18n="projectlist.label.notworkingupdate"></span></p>
						<div class="popupCloseBtn">
							<input type="button" value="Close" data-i18n="[value]common.btn.close" class="btn btn_style dyn_popup_close">
						</div>		
					 </div>	
                    <!--svn_update div ends here -->	
                  </div>
                  </td>
                  <td class="list_img">
                  	{{#if ../../userPermissions.manageApplication}}
	                  	<a name="{{creationDate}}" dynamicid="{{id}}"  class="tooltiptop del_appln" title="" data-placement="bottom" data-toggle="tooltip" href="javascript:void(0)" data-original-title="Delete {{appDirName}}">
	                  		<img name="delete" src="themes/default/images/Phresco/delete_row.png" width="14" height="18" border="0" alt="">
	              		</a>
              		{{else}}
              			<img name="delete" src="themes/default/images/Phresco/delete_row_off.png" width="14" height="18" border="0" alt="">
              		{{/if}}
                    <div id="{{creationDate}}" class="dyn_popup delete_msg" currentprjname={{appDirName}}>
                      Are you sure to delete ?
                        <div>
                            <input type="button" value="Yes" data-i18n="[value]common.btn.yes" class="btn btn_style dlt" name="deleteConfirm" deleteAppname="{{appDirName}}">
                            <input type="button" value="No" data-i18n="[value]common.btn.no" class="btn btn_style dyn_popup_close">
                        </div>
                    </div>
                  </td>
                        <td>&nbsp;</td>
                      </tr>
					  {{#each modules}}
                          <tr class="{{../appDirName}}" techid="{{techInfo.id}}" isMultiModule="true" moduleName="{{code}}">
                              <td style="padding-left: 5%;"><a name="editApplication" from="multimodule" appDirName="{{../appDirName}}" href="javascript:;">{{code}}</a></td>
                              <td>&nbsp;</td>
                              <td>{{techInfo.name}}</td>
                              <td colspan="2" class="list_img">
                                  <a name="pdf_report_{{id}}" temp="pdf_report" fromPage="All" class="tooltiptop" title="" dynamicId="{{id}}" data-placement="bottom" data-t="tooltip" href="javascript:void(0)" data-original-title="PDF Report">
                                      <img src="themes/default/images/Phresco/pdf_icon.png" width="14" height="16" border="0" alt="">
                                  </a>
                                                
                                  <div id="pdf_report_{{id}}" class="dyn_popup pdf_report">
                                      <div id="noReport_{{id}}" class="alert noReport hideContent" style="text-align: center;"></div>
										<table class="hideContent table" name="pdfHeader_{{id}}" show="true">
											  <thead>  
												<tr>
													<th class="" data-i18n="commonlabel.label.existingreport"></th>
													<th class="" data-i18n="commonlabel.label.type"></th>
													<th class="" data-i18n="commonlabel.label.download"></th>
													<th class="" data-i18n="commonlabel.label.delete"></th>
												</tr>
											  </thead>			
									  </table>
                                      <div class = "pdfheight">
										  <table class="existing_report" width="100%" border="0" cellspacing="0" cellpadding="5">
												<tbody name="generatedPdfs_{{id}}" class="scroll"></tbody>    
										  </table>
									  </div>
                                      <form id="pdfReport">
                                          <table class="report_type">
                                              <tbody>
                                                  <tr>
                                                      <td data-i18n="commonlabel.label.reporttype"></td>
                                                      <td>
                                                          <select name="reportDataType" class="reportDataType">
                                                              <option value="detail" data-i18n="projectlist.label.detailedReport"></option>
                                                              <option value="crisp" data-i18n="projectlist.label.overallReport"></option>
                                                          </select>
                                                      </td>
                                                  </tr>
                                                  <tr>
                                                      <td data-i18n="commonlabel.label.pdfreportname"></td>
                                                      <td>
                                                          <input type="text" data-i18n="[placeholder]projectlist.label.pdfreportname" name="pdfName" class="pdfName">
                                                      </td>
                                                  </tr>
                                              </tbody>
                                          </table>
                                          <div class="popuploading" id="pdfReportLoading_{{id}}">&nbsp;</div>
                                          <div class="flt_right">
                                              {{#if ../../../userPermissions.managePdfReports}}
                                                  <input type="button" fromPage="All"  appDirName="{{../../appDirName}}" value="Generate" data-i18n="[value]projectlist.label.generate" class="btn btn_style" moduleName="{{code}}" name="generate" appId="{{id}}" projectId = "{{../id}}">
                                              {{else}}
                                                  <input type="button" disabled=true fromPage="All"  appDirName="{{../../appDirName}}" value="Generate" data-i18n="[value]projectlist.label.generate" class="btn btn_style" name="generate" moduleName="{{code}}" appId="{{id}}" projectId = "{{../id}}">
                                              {{/if}}
                                              <input type="button" value="Close" data-i18n="[value]common.btn.close" class="btn btn_style dyn_popup_close">
                                          </div>
                                          <input type="hidden" name="fromPage" value="All" data-i18n="[value]common.btn.all"/>
                                      </form>
                                      <!--pdf_report div ends here -->  
                                  </div> 
                              </td>
                              <td class="list_img">
                                  {{#if ../../../userPermissions.manageApplication}}
                                      <a class="tooltiptop" title="" name="{{code}}" data-placement="bottom" data-toggle="tooltip" href="#" data-original-title="Delete {{code}}">
                                          <img name="delete" src="themes/default/images/Phresco/delete_row.png" width="14" height="18" border="0" alt="">
                                      </a>
                                  {{else}}
                                      <img name="delete" src="themes/default/images/Phresco/delete_row_off.png" width="14" height="18" border="0" alt="">
                                  {{/if}}
                                      <div class="dyn_popup delete_msg" id="{{code}}" currentModuleName={{code}}>
                                          Are you sure to delete ?
                                        <div>
                                            <input type="button" value="Yes" data-i18n="[value]common.btn.yes" class="btn btn_style dlt" from="module" appDirName="{{../appDirName}}" name="deleteConfirm" moduleName="{{code}}">
                                            <input type="button" value="No" data-i18n="[value]common.btn.no" class="btn btn_style dyn_popup_close">
                                        </div>
                                      </div>
                              </td>
                          </tr>
                      {{/each}}
                    {{/each}}
                  {{/each}}
              </tbody>
            </table>
			</div>
			</div>
			{{else}}
				<div class="alert" style="text-align: center;" data-i18n="projectlist.label.nodata"></div>
			{{/if}}
            </section>
			<section class="content_end" style="display:none;">
			<div class="msgdisplay"></div>
			</section>
          