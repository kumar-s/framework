<section class="content_main proj_list">
	{{#if projectlist}}
		<table class="table table-striped table_border table-bordered" id="projectlist" cellpadding="0" cellspacing="0" border="0">
              <thead class="fixedHeader">
                 <tr>
                  <th data-i18n="projectlist.label.name"></th>
                  <th data-i18n="projectlist.label.description"></th>
                  <th data-i18n="projectlist.label.technology"></th>
                  <th data-i18n="projectlist.label.report"></th>
                  <th data-i18n="projectlist.label.repository"></th>
                  <th>Delete</th>                  
                  <th>&nbsp;</th>
                </tr>
              </thead>
              <tbody class="scrollContent">
                  {{#each projectlist}}
                      <tr class="proj_title"> 
                        <td colspan="4">{{name}}</td>  
                        <td colspan="3" align="right" class="delimages">
                        	<a href="#" class="tooltiptop" title="" data-placement="bottom" data-toggle="tooltip"  data-original-title="Edit Project">
                        		<img name="editproject" key="{{id}}" src="themes/default/images/helios/edit_row_icon.png" width="29" height="23" border="0" alt="">
                    		</a> 
                    		{{#if ../userPermissions.manageApplication}}
	                    		<a name="{{id}}" class="tooltiptop" title="" data-placement="bottom" data-toggle="tooltip" href="#" data-original-title="Delete Row">
	                    			<img src="themes/default/images/helios/delete_row_icon.png" width="29" height="23" border="0" alt="0">
	                			</a>
                			{{else}}
                				<img src="themes/default/images/helios/delete_row_icon_off.png" width="29" height="23" border="0" alt="0">
                			{{/if}}

                            <div id="{{id}}" class="dyn_popup deleteproj_msg"> Are you sure to delete this ?
                                <div>
                                    <input type="button" value="Yes" class="btn btn_style" name="holeDelete">
                                    <input type="button" value="No" class="btn btn_style dyn_popup_close">
                                </div>
                            </div>
                        </td>
                      </tr>
					         {{#each appInfos}}
                      <tr class="{{appDirName}}" techid="{{techInfo.id}}">
                        <td><a name="editApplication" href="javascript:;">{{name}}</a></td>
                        <td>{{#if description }} {{description}} {{else}} &nbsp {{/if}}</td>
                        <td>{{techInfo.name}}</td>
                       <td><a name="pdf_report" fromPage="All" class="tooltiptop" title="" dynamicId="{{id}}" data-placement="top" data-t="tooltip" href="#" data-original-title="PDF Report"><img src="themes/default/images/helios/pdf_icon.png" width="14" height="16" border="0" alt=""></a>
                  
              <div id="pdf_report" class="dyn_popup pdf_report">
              <div id="noReport" class="noReport"></div>
                <table class="existing_report" width="100%" border="0" cellspacing="0" cellpadding="5">
                  <thead name="pdfHeader">
                        <tr>
                            <th>Existing Reports</th>
                            <th>Type</th>
                            <th>Download</th>
                            <th>Delete</th>
                        </tr>    
                    </thead>
                    <tbody name="generatedPdfs">
                        <tr>
                            <td>Apr 17 2013 12:51</td>
                            <td>Detailed</td>
                            <td><img src="themes/default/images/helios/download_icon.png" width="15" height="18" border="0" alt="0"></td>
                            <td><img name="delete" src="themes/default/images/helios/delete_row.png" width="14" height="18" border="0" alt="0"></td>
                        </tr>
                    </tbody>    
                </table>
                   <table class="report_type">
                    <tbody>
                        <tr>
                            <td>Report Type</td>
                            <td>
                                <select id="reportType">
                                    <option value="detailedReport">Detailed Report</option>
                                    <option value="overallReport">Overall Report</option>
                                </select>
                            </td>
                        </tr>
                         <tr>
                            <td>Pdf Report Name</td>
                            <td>
                               <input type="text" placeholder="Pdf report name" name="pdfName">
                            </td>
                        </tr>
                    </tbody>
                   </table>
                   <div class="popuploading" id="pdfReportLoading_{{id}}">&nbsp</div>
                <div class="flt_right">
                 <input type="button" fromPage="All" sonarUrl="http://localhost:9000/dashboard" value="Generate" class="btn btn_style" name="generate" appId="{{id}}" projectId = "{{../id}}">
                 <input type="button" value="Close" class="btn btn_style dyn_popup_close">
               </div>
                <!--pdf_report div ends here -->  
             </div> 
                  </td>
                        <td>
                        {{#if ../../userPermissions.manageRepo}}
							<a name="addRepo_{{id}}" dynamicId="{{id}}" class="tooltiptop" title="" data-placement="top" data-toggle="tooltip" href="#" data-original-title="Add Repo" >
								<img src="themes/default/images/helios/add_repo_icon.png" width="18" height="15" border="0" alt="">
							</a> 

                        	<a name="commit{{id}}" dynamicId="{{id}}" class="tooltiptop" title="" data-placement="top" data-toggle="tooltip" href="#" data-original-title="Commit" >
                        		<img src="themes/default/images/helios/commit_icon.png" width="16" height="15" border="0" alt="">
                    		</a> 
						{{else}}
                        	<img src="themes/default/images/helios/add_repo_icon_off.png" width="18" height="15" border="0" alt="">
                        	<img src="themes/default/images/helios/commit_icon_off.png" width="16" height="15" border="0" alt="">
                        {{/if}}
                        
                        {{#if ../../userPermissions.manageRepo}}
                        	<a name="svn_update{{id}}" dynamicId="{{id}}" class="tooltiptop" title="" data-placement="top" data-toggle="tooltip" href="#" data-original-title="Update" >
                        		<img src="themes/default/images/helios/update_svn_icon.png" width="22" height="15" border="0" alt="">
                    		</a>
              			{{else}}
              				<img src="themes/default/images/helios/update_svn_icon_off.png" width="22" height="15" border="0" alt="">
              			{{/if}}
                  
                  <div id="addRepo_{{id}}" class="add_repo dyn_popup" name="addRepo">
                      <table width="100%" border="0" cellspacing="0" cellpadding="0">
                        <tbody>
                            <tr>
                                <td>Type<sup>*</sup></td>
                                <td>Application Repo URL<sup>*</sup></td>
                            </tr>
                            <tr>
                                <td>
                                    <select id="type_{{id}}">
                                        <option value="svn">SVN</option>
                                        <option value="bitkeeper">Bitkeeper</option>
                                        <option value="git">Git</option>
                                    </select>
                                </td>
                                <td><input type="text" id="repourl_{{id}}"></td>
                            </tr>
                            <tr>
                                <td class="seperatetd">Other Credential<input type="checkbox" id="repocredential_{{id}}" repoDynid="{{id}}" class="credential"></td>
                                <td></td>
                            </tr>
                            <tr>
                                <td>Username</td>
                                <td>Password</td>
                            </tr>
                            <tr>
                                <td><input type="text" placeholder="username" id="uname_{{id}}" class="uname"></td>
                                <td><input type="password" placeholder="*******" id="pwd_{{id}}" class="pwd"></td>
                            </tr>
                            <tr>
                                <td colspan="2">
                                    <textarea class="popuptextarea" placeholder="Message" id="repomessage_{{id}}"></textarea>
                                </td>
                            </tr>
                        </tbody>
                     </table> 
                     <div class="popuploading" id="addRepoLoading_{{id}}">&nbsp</div>
                     <div class="flt_right" appId="{{id}}" projectId = "{{../id}}" appDirName="{{appDirName}}">
                        <input type="button" name="addrepobtn" id="{{id}}" value="Add To Repo" class="btn btn_style">
                        <input type="button" value="Close" class="btn btn_style dyn_popup_close">
                     </div> 
                  </div>
                  <!--add_repo div ends here --> 
                  
                  <div id="commit{{id}}" class="commit dyn_popup popup_bg">
                      <div class="hideContent commit_data_{{id}}">
                            <table  class="commitable_files_{{id}}" width="100%" border="0" cellspacing="0" cellpadding="5">
                                  
                            </table>   
                            <br>
                            <table class="commitData_{{id}}" width="100%" border="0" cellspacing="0" cellpadding="0">
                                <tbody>
                                    <tr>
                                        <td>Type<sup>*</sup></td>
                                        <td>Application Repo URL<sup>*</sup></td>
                                    </tr>
                                    <tr>
                                        <td>
                                            <select id="commitType_{{id}}">
                                                <option value="svn">SVN</option>
                                                <option value="bitkeeper">Bitkeeper</option>
                                                <option value="git">Git</option>
                                            </select>
                                        </td>
                                        <td><input type="text" id="commitRepourl_{{id}}"></td>
                                    </tr>
                                    <tr>
                                        <td class="seperatetd">Other Credential<input type="checkbox" id="commitCredential_{{id}}" class="credential"></td>
                                        <td></td>
                                    </tr>
                                    <tr>
                                        <td>Username</td>
                                        <td>Password</td>
                                    </tr>
                                    <tr>
                                        <td><input type="text" placeholder="admin" id="commitUsername_{{id}}" class="uname"></td>
                                        <td><input type="password" placeholder="*******" id="commitPassword_{{id}}" class="pwd"></td>
                                    </tr>
                                    <tr>
                                        <td colspan="2">
                                            <textarea class="popuptextarea" placeholder="Message" id="commitMessage_{{id}}"></textarea>
                                        </td>
                                    </tr>
                                </tbody>
                             </table> 
							 
                         <div class="flt_right roll_data" appDirName="{{appDirName}}">
                          <input type="button" value="Commit" disabled class="btn" id="{{id}}" name="commitbtn">
                            <input type="button" value="Close" class="btn btn_style dyn_popup_close">
                         </div>
					 </div> 
				 	<div class="dummyCommit" id="dummyCommit_{{id}}"></div>
				 	<div class="popuploading" id="commitLoading_{{id}}">&nbsp</div>
					<div class="hideContent commitErr_{{id}} popup_btn" width="100%" border="0" cellspacing="0" cellpadding="0">
						<p>This Project is not a working copy, add to repository and then try to commit.</p>
						<div class="popupCloseBtn">
							<input type="button" value="Close" class="btn btn_style dyn_popup_close">
						</div>		
					 </div>
                  </div>
                   <!--commit div ends here -->	
                   
                  <div id="svn_update{{id}}" class="svn_update dyn_popup popup_bg">
					<table width="100%" border="0" cellspacing="0" cellpadding="0">
                        <tbody>
                                    <tr>
                                        <td>Type<sup>*</sup></td>
                                        <td>Application Repo URL<sup>*</sup></td>
                                    </tr>
                                    <tr>
                                        <td>
                                            <select id="updateType_{{id}}">
                                                <option value="svn">SVN</option>
                                                <option value="bitkeeper">Bitkeeper</option>
                                                <option value="git">Git</option>
                                            </select>
                                        </td>
                                        <td><input type="text" id="updateRepourl_{{id}}"></td>
                                    </tr>
                                    <tr>
                                        <td class="seperatetd">Other Credential<input type="checkbox" id="updateCredential_{{id}}" class="credential"></td>
                                        <td></td>
                                    </tr>
                                    <tr>
                                        <td>Username</td>
                                        <td>Password</td>
                                    </tr>
                                    <tr>
                                        <td><input type="text" placeholder="admin" id="updateUsername_{{id}}" class="uname"></td>
                                        <td><input type="password" placeholder="*******" id="updatePassword_{{id}}" class="pwd"></td>
                                    </tr>
                                    <tr>
                                        <td colspan="2">Revision</td>
                                    </tr>
                                    <tr>
                                        <td><input type="radio" name="revision" class="radioBtnClass" value="head">Head Revision<input type="radio" name="revision" class="radioBtnClass" value="">Revision</td>
                                        <td><input type="text"></td>
                                    </tr>
                                </tbody>
                     </table> 
                     <div class="popuploading" id="updateRepoLoading_{{id}}">&nbsp</div>
                     <div class="flt_right" appDirName="{{appDirName}}">
                         <input type="button" value="Update Application" id="{{id}}" class="btn btn_style" name="updatebtn">
                         <input type="button" value="Close" class="btn btn_style dyn_popup_close">
                     </div>               
                    <!--svn_update div ends here -->	
                  </div>
                  </td>
                  <td>
                  	{{#if ../../userPermissions.manageApplication}}
	                  	<a name="{{creationDate}}" class="tooltiptop" title="" data-placement="top" data-toggle="tooltip" href="#" data-original-title="Delete Row">
	                  		<img name="delete" src="themes/default/images/helios/delete_row.png" width="14" height="18" border="0" alt="">
	              		</a>
              		{{else}}
              			<img name="delete" src="themes/default/images/helios/delete_row_off.png" width="14" height="18" border="0" alt="">
              		{{/if}}
                    <div id="{{creationDate}}" class="dyn_popup delete_msg">
                      Are you sure to delete ?
                        <div>
                            <input type="button" value="Yes" class="btn btn_style dlt" name="deleteConfirm">
                            <input type="button" value="No" class="btn btn_style dyn_popup_close">
                        </div>
                    </div>
                  </td>
                        <td>&nbsp;</td>
                      </tr>
                    {{/each}}
                  {{/each}}
              </tbody>
            </table>			
			{{else}}
				<div class="alert" style="text-align: center;" data-i18n="projectlist.label.nodata"></div>
			{{/if}}
            </section>
          