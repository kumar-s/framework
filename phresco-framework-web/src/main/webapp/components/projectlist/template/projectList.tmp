                        <td colspan="4">{{name}}</td>  
                        <td colspan="3" align="right" class="delimages">
                        	<a href="#" class="tooltiptop" title="" data-placement="bottom" data-toggle="tooltip"  data-original-title="Edit Project">
                        		<img name="editproject" key="{{id}}" src="themes/default/images/helios/edit_row_icon.png" width="29" height="23" border="0" alt="">
                    		</a> 
                    		{{#if ../userPermissions.manageApplication}}
	                    		<a name="{{id}}" class="tooltiptop" title="" data-placement="bottom" data-toggle="tooltip" href="#" data-original-title="Delete Row">
	                    			<img src="themes/default/images/helios/delete_row_icon.png" width="29" height="23" border="0" alt="0">
	                			</a>
                			{{else}}
                				<img src="themes/default/images/helios/delete_row_icon_off.png" width="29" height="23" border="0" alt="0">
                			{{/if}}

                            <div id="{{id}}" class="dyn_popup"> Are you sure to delete this ?
