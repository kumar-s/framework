<div class="second_title" >
    	<div class="features_right">
            <font data-i18n="features.show"></font>
            <fieldset class="switch switchOff" value="false">
            <label class="off" name="on_off" value="false" id="switchonbutton"></label>
            <label class="on" name="on_off" value="true" id="switchoffbutton"></label>
            </fieldset>
    	</div> 
    	<!--second_title div ends here -->
    </div>
    
    <section class="features_content_main z_index">
        
        <table width="100%" cellpadding="0" cellspacing="0" border="0" id="featureTest">
        <tr>
            {{#if featureslist}}
            <td>
                <div class="features_box">
                <h1 data-i18n="features.list.modules"></h1>
                <input type="text" class="flt_right" data-i18n="[placeholder]features.placeholder" id="module">
                <div class="clear"></div>
                <div class="box_div" id="content_1">
                    <ul id="moduleContent">
                    {{#each featureslist}}
                        <li name="{{name}}" dispName="{{dispName}}" packaging="{{packaging}}" type="{{type}}">
                        <span class="flt_right" ><a class="featureinfo_img" value="{{#idtrime id}}{{/idtrime}}" artifactGroupId="{{id}}" > 
                        <img src="themes/default/images/helios/info_icon.png" width="15" height="16" border="0" alt=""></a></span>
                                {{#versiondata versions}}{{/versiondata}}
								{{#versionShowHide versions}}{{/versionShowHide}} 
								{{#each versions}}
									{{#each appliesTo}}
										<input type="hidden" defaultModule="true">
									{{/each}}
								{{/each}}	
								<input type="hidden" class="moduleId" value="{{id}}">
                                <select class="input-mini">
                                    {{#each versions}}									
                                        <option value="{{id}}" scope="{{scope}}">{{version}}</option>  
                                    {{/each}} 
                                </select>
                            </div>
                             {{#if displayName}}
                                {{displayName}}
                            {{else}}
                                {{name}}
                            {{/if}}
                            <div class="clear"></div>
                        </li>
						
                  {{/each}}  
					<li id="norecord1" style="display:none" align="center">No records</li>				  
                    </ul>   
                
                </div>
                <!--modules div ends here -->
            </div>  
            </td>
            {{/if}}
            {{#if jsibrarielist}}
            <td>
                <div class="features_box">
                <h1 data-i18n="features.list.jslibraries"></h1>
                <input type="text" class="flt_right" data-i18n="[placeholder]features.placeholder" id="jsibraries">
                <div class="clear"></div>
                <div class="box_div" id="content_2">
                   <ul id="jsibrariesContent">
                    {{#each jsibrarielist}}
                        <li name="{{name}}" dispName="{{dispName}}" packaging="{{packaging}}" type="{{type}}">
                        <span class="flt_right" ><a class="featureinfo_img" value="{{#idtrime id}}{{/idtrime}}" artifactGroupId="{{id}}"> 
                        <img src="themes/default/images/helios/info_icon.png" width="15" height="16" border="0" alt=""></a></span>
                                {{#versiondata versions}}{{/versiondata}} 
								{{#versionShowHide versions}}{{/versionShowHide}} 
								{{#each versions}}
									{{#each appliesTo}}
										<input type="hidden" defaultModule="true">
									{{/each}}
								{{/each}}	
								<input type="hidden" class="moduleId" value="{{id}}">
                                <select class="input-mini">
                                    {{#each versions}}										
                                        <option value="{{id}}" scope="{{scope}}">{{version}}</option>  
                                    {{/each}} 
                                </select>
                            </div>
                             {{#if displayName}}
                                {{displayName}}
                            {{else}}
                                {{name}}
                            {{/if}}
                            <div class="clear"></div>
                        </li>
                  {{/each}}
					<li id="norecord2" style="display:none" align="center">No records</li>
                    </ul>   
                
                </div>
                <!--Js Libraries div ends here -->
            </div>
            </td>
            {{/if}}

            {{#if componentList}}
            <td>
                <div class="features_box">
                <h1 data-i18n="features.list.components"></h1>
                <input type="text" class="flt_right" data-i18n="[placeholder]features.placeholder" id="components">
                <div class="clear"></div>
                <div class="box_div" id="content_3">
                    <ul id="componentsContent">
                    {{#each componentList}}
                        <li name="{{name}}" dispName="{{dispName}}" packaging="{{packaging}}" type="{{type}}">
                        <span class="flt_right" ><a class="featureinfo_img" value="{{#idtrime id}}{{/idtrime}}" artifactGroupId="{{id}}"> 
                        <img src="themes/default/images/helios/info_icon.png" width="15" height="16" border="0" alt=""></a></span>
                                {{#versiondata versions}}{{/versiondata}} 
								{{#versionShowHide versions}}{{/versionShowHide}} 
								{{#each versions}}
									{{#each appliesTo}}
										<input type="hidden" defaultModule="true">
									{{/each}}
								{{/each}}	
								<input type="hidden" class="moduleId" value="{{id}}">
                                <select class="input-mini">
                                    {{#each versions}}										
                                        <option value="{{id}}" scope="{{scope}}">{{version}}</option>  
                                    {{/each}} 
                                </select>
                            </div>
                             {{#if displayName}}
                                {{displayName}}
                            {{else}}
                                {{name}}
                            {{/if}}
                            <div class="clear"></div>
                        </li>
                  {{/each}}
					<li id="norecord3" style="display:none" align="center">No records</li>
                    </ul>   
                
                </div>
                <!--Components div ends here -->
            </div>
            </td>
            {{/if}}
        </tr>
        </table>
        
        <!--features_content_main div ends here -->
    </section>

    <div id="desc"></div>
    
    <div id="repgen_info" class="dyn_popup featureinfo">
        <h1>Description</h1><a href="#" class="dyn_popup_close">X</a>
        <div class="features_cont">
        	<span><img src="themes/default/images/helios/feature_info_logo.png" width="42" height="42" border="0" alt=""></span>
            Cordova provides a set of uniform JavaScript libraries that can be invoked, with device-specific native backing.
        </div> 
    </div>
    <section class="content_end">
        <div class="flt_right">
        	{{#if userPermissions.manageApplication}}
        		<input type="submit" data-i18n="[value]features.btn.update" class="btn btn_style update_btn" id="featureUpdate">
    		{{else}}
            	<input type="submit" data-i18n="[value]features.btn.update" class="btn btn_style update_btn" id="featureUpdate" disabled>
        	{{/if}}
        	<input type="submit" id="cancelUpdateFeature" data-i18n="[value]features.btn.cancel" class="btn btn_style cancel_icon_btn">
        </div>
    	<!--content_title ends here -->
    </section>
